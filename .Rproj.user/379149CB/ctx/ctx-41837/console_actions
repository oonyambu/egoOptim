        "\n[133] \"                           opt = list(x=rep(0, d), f = 0)),\"                                          \n[134] \"  shekel = list(lower = rep(0, 4), upper = rep(10, 4),\"                                               \n[135] \"                opt = list(x = rep(4, 4),\"                                                            \n[136] \"                           f=setNames(c(-10.1532, -10.4029, -10.5364),\"                               \n[137] \"                                      paste0(\\\"m=\\\", c(5,7,10))))),\"                                  ",
        "\n[138] \"  stybtang = function(d) list(lower = rep(-5, d), upper = rep(5, d),\"                                 \n[139] \"                              opt = list(x = rep(-2.903534, d),\"                                      \n[140] \"                                         f = -39.16599*d)))\"                                          \n[141] \"  if(is.null(fun_name))dm\"                                                                            \n[142] \"  else dm[[fun_name]]\"                                                                                ",
        "\n[143] \"}\"                                                                                                    \n\n$`./R/drop.R`\n [1] \"#' DROP-WAVE FUNCTION\"                                                                                                                                                       \n [2] \"#'\"                                                                                                                                                                          \n [3] \"#' Dimensions: 2\"                                                                                                                                                            ",
        "\n [4] \"#' \\tThe Drop-Wave function is multimodal and highly complex. The second plot above shows the function on a smaller input domain, to illustrate its characteristic features.\"\n [5] \"#' \\tThe function is usually evaluated on the square xi <U+2208> [-5.12, 5.12], for all i = 1, 2.\"                                                                           \n [6] \"#' \\tReference:\"                                                                                                                                                             ",
        "\n [7] \"#' @author Sonja Surjanovic, Simon Fraser University\"                                                                                                                        \n [8] \"#'          \\tDerek Bingham, Simon Fraser University\"                                                                                                                        \n [9] \"#' @param xx c(x1, x2)\"                                                                                                                                                      ",
        "\n[10] \"#' @export drop\"                                                                                                                                                             \n[11] \"#'\"                                                                                                                                                                          \n[12] \"\"                                                                                                                                                                            ",
        "\n[13] \"drop <- function(xx)\"                                                                                                                                                        \n[14] \"{\"                                                                                                                                                                           \n[15] \"  \"                                                                                                                                                                          ",
        "\n[16] \"  x1 <- xx[1]\"                                                                                                                                                               \n[17] \"  x2 <- xx[2]\"                                                                                                                                                               \n[18] \"\\t\"                                                                                                                                                                          ",
        "\n[19] \"  frac1 <- 1 + cos(12*sqrt(x1^2+x2^2))\"                                                                                                                                      \n[20] \"  frac2 <- 0.5*(x1^2+x2^2) + 2\"                                                                                                                                              \n[21] \"\\t\"                                                                                                                                                                          ",
        "\n[22] \"  y <- -frac1/frac2\"                                                                                                                                                         \n[23] \"  return(y)\"                                                                                                                                                                 \n[24] \"}\"                                                                                                                                                                           ",
        "\n\n$`./R/easom.R`\n [1] \"#' EASOM FUNCTION\"                                                                                                                      \n [2] \"#'\"                                                                                                                                     \n [3] \"#' Dimensions: 2\"                                                                                                                       \n [4] \"#' \\tThe Easom function has several local minima. It is unimodal, and the global minimum has a small area relative to the search space.\"",
        "\n [5] \"#' \\tThe function is usually evaluated on the square xi <U+2208> [-100, 100], for all i = 1, 2.\"                                        \n [6] \"#' \\tReference:\"                                                                                                                        \n [7] \"#' \\thttp://www-optima.amp.i.kyoto-u.ac.jp/member/student/hedar/Hedar_files/TestGO.htm.\"                                                \n [8] \"#' @author Sonja Surjanovic, Simon Fraser University\"                                                                                   ",
        "\n [9] \"#'          \\tDerek Bingham, Simon Fraser University\"                                                                                   \n[10] \"#' @param xx c(x1, x2)\"                                                                                                                 \n[11] \"#' @export easom\"                                                                                                                       \n[12] \"#'\"                                                                                                                                     ",
        "\n[13] \"\"                                                                                                                                       \n[14] \"easom <- function(xx)\"                                                                                                                  \n[15] \"{\"                                                                                                                                      \n[16] \"  \"                                                                                                                                     ",
        "\n[17] \"  x1 <- xx[1]\"                                                                                                                          \n[18] \"  x2 <- xx[2]\"                                                                                                                          \n[19] \"\\t\"                                                                                                                                     \n[20] \"  fact1 <- -cos(x1)*cos(x2)\"                                                                                                            ",
        "\n[21] \"  fact2 <- exp(-(x1-pi)^2-(x2-pi)^2)\"                                                                                                   \n[22] \"\\t\"                                                                                                                                     \n[23] \"  y <- fact1*fact2\"                                                                                                                     \n[24] \"  return(y)\"                                                                                                                            ",
        "\n[25] \"}\"                                                                                                                                      \n\n$`./R/egg.R`\n [1] \"#' EGGHOLDER FUNCTION\"                                                                                        \n [2] \"#'\"                                                                                                           \n [3] \"#' Dimensions: 2\"                                                                                             \n [4]",
        " \"#' \\tThe Eggholder function is a difficult function to optimize, because of the large number of local minima.\"\n [5] \"#' \\tThe function is usually evaluated on the square xi <U+2208> [-512, 512], for all i = 1, 2.\"              \n [6] \"#' \\tReference:\"                                                                                              \n [7] \"#' @author Sonja Surjanovic, Simon Fraser University\"                                                         \n [8] \"#'          \\tDerek Bingham, Simon Fraser University\"                                                         ",
        "\n [9] \"#' @param xx c(x1, x2)\"                                                                                       \n[10] \"#' @export egg\"                                                                                               \n[11] \"#'\"                                                                                                           \n[12] \"\"                                                                                                             \n[13] \"egg <- function(xx)\"                                                                                          ",
        "\n[14] \"{\"                                                                                                            \n[15] \"  \"                                                                                                           \n[16] \"  x1 <- xx[1]\"                                                                                                \n[17] \"  x2 <- xx[2]\"                                                                                                \n[18] \"\\t\"                                                                                                           ",
        "\n[19] \"  term1 <- -(x2+47) * sin(sqrt(abs(x2+x1/2+47)))\"                                                             \n[20] \"  term2 <- -x1 * sin(sqrt(abs(x1-(x2+47))))\"                                                                  \n[21] \"\\t\"                                                                                                           \n[22] \"  y <- term1 + term2\"                                                                                         \n[23] \"  return(y)\"                                                                                                  ",
        "\n[24] \"}\"                                                                                                            \n\n$`./R/forretal08.R`\n [1] \"#' FORRESTER ET AL. (2008) FUNCTION\"                                                                                                                                                                    \n [2] \"#'\"                                                                                                                                                                                                     ",
        "\n [3] \"#' Dimensions: 1\"                                                                                                                                                                                       \n [4] \"#' \\tThis function is a simple one-dimensional test function. It is multimodal, with one global minimum, one local minimum and a zero-gradient inflection point.\"                                       \n [5] \"#' \\tThe function is evaluated on x <U+2208> [0, 1].\"                                                                                                                                                   ",
        "\n [6] \"#' \\tModifications and Alternative Forms:\"                                                                                                                                                              \n [7] \"#' \\tFor the purpose of multi-fidelity analysis, the following function is used for the lower fidelity code:\"                                                                                           \n [8] \"#' \\tHere, the constants A, B and C can be varied to improve the fidelity of the low-fidelity function. In the plot above, the values used are (as in Forrester et al., 2008): A = 0.5, B = 10, C = -5.\"",
        "\n [9] \"#' \\tReference:\"                                                                                                                                                                                        \n[10] \"#' \\tForrester, A., Sobester, A., & Keane, A. (2008). Engineering design via surrogate modelling: a practical guide. Wiley.\"                                                                            \n[11] \"#' @author Sonja Surjanovic, Simon Fraser University\"                                                                                                                                                   ",
        "\n[12] \"#'          \\tDerek Bingham, Simon Fraser University\"                                                                                                                                                   \n[13] \"#' @export forretal08\"                                                                                                                                                                                  \n[14] \"#'\"                                                                                                                                                                                                     ",
        "\n[15] \"\"                                                                                                                                                                                                       \n[16] \"forretal08 <- function(x)\"                                                                                                                                                                              \n[17] \"{\"                                                                                                                                                                                                      ",
        "\n[18] \"  \"                                                                                                                                                                                                     \n[19] \"  fact1 <- (6*x - 2)^2\"                                                                                                                                                                                 \n[20] \"  fact2 <- sin(12*x - 4)\"                                                                                                                                                                               ",
        "\n[21] \"  \"                                                                                                                                                                                                     \n[22] \"  y <- fact1 * fact2\"                                                                                                                                                                                   \n[23] \"  return(y)\"                                                                                                                                                                                            ",
        "\n[24] \"}\"                                                                                                                                                                                                      \n\n$`./R/forretal08lc.R`\n [1] \"#' FORRESTER ET AL. (2008) FUNCTION, LOWER FIDELITY CODE\"                                                                                                                                               \n [2] \"#'\"                                                                                                                                                                                                     ",
        "\n [3] \"#' Dimensions: 1\"                                                                                                                                                                                       \n [4] \"#' \\tThis function is a simple one-dimensional test function. It is multimodal, with one global minimum, one local minimum and a zero-gradient inflection point.\"                                       \n [5] \"#' \\tThe function is evaluated on x <U+2208> [0, 1].\"                                                                                                                                                   ",
        "\n [6] \"#' \\tModifications and Alternative Forms:\"                                                                                                                                                              \n [7] \"#' \\tFor the purpose of multi-fidelity analysis, the following function is used for the lower fidelity code:\"                                                                                           \n [8] \"#' \\tHere, the constants A, B and C can be varied to improve the fidelity of the low-fidelity function. In the plot above, the values used are (as in Forrester et al., 2008): A = 0.5, B = 10, C = -5.\"",
        "\n [9] \"#' \\tReference:\"                                                                                                                                                                                        \n[10] \"#' \\tForrester, A., Sobester, A., & Keane, A. (2008). Engineering design via surrogate modelling: a practical guide. Wiley.\"                                                                            \n[11] \"#' \\tCalls: forretal08.r\"                                                                                                                                                                               ",
        "\n[12] \"#' \\tThis function is used as the \\\"low-accuracy code\\\" version of the function\"                                                                                                                        \n[13] \"#' \\tforretal08.r.\"                                                                                                                                                                                     \n[14] \"#' @author Sonja Surjanovic, Simon Fraser University\"                                                                                                                                                   ",
        "\n[15] \"#'          \\tDerek Bingham, Simon Fraser University\"                                                                                                                                                   \n[16] \"#' @param x scalar\"                                                                                                                                                                                     \n[17] \"#' @param A constant (optional), with default value 0.5\"                                                                                                                                                ",
        "\n[18] \"#' @param B constant (optional), with default value 10\"                                                                                                                                                 \n[19] \"#' @param C constant (optional), with default value -5\"                                                                                                                                                 \n[20] \"#' @export forretal08lc\"                                                                                                                                                                                ",
        "\n[21] \"#'\"                                                                                                                                                                                                     \n[22] \"\"                                                                                                                                                                                                       \n[23] \"forretal08lc <- function(x, A=0.5, B=10, C=-5)\"                                                                                                                                                         ",
        "\n[24] \"{\"                                                                                                                                                                                                      \n[25] \"  \"                                                                                                                                                                                                     \n[26] \"  source('forretal08.r')\"                                                                                                                                                                               ",
        "\n[27] \"  yh <- forretal08(x)\"                                                                                                                                                                                  \n[28] \"  \"                                                                                                                                                                                                     \n[29] \"  term1 <- A * yh\"                                                                                                                                                                                      ",
        "\n[30] \"  term2 <- B * (x-0.5)\"                                                                                                                                                                                 \n[31] \"  \"                                                                                                                                                                                                     \n[32] \"  y <- term1 + term2 - C\"                                                                                                                                                                               ",
        "\n[33] \"  return(y)\"                                                                                                                                                                                            \n[34] \"}\"                                                                                                                                                                                                      \n\n$`./R/goldpr.R`\n [1] \"#' GOLDSTEIN-PRICE FUNCTION\"                                                                                                                                     ",
        "\n [2] \"#'\"                                                                                                                                                              \n [3] \"#' Dimensions: 2\"                                                                                                                                                \n [4] \"#' \\tThe Goldstein-Price function has several local minima.\"                                                                                                     \n [5] \"#' \\tThe function is usually evaluated on the square xi <U+2208> [-2, 2], for all i = 1, 2.\"                                                                     ",
        "\n [6] \"#' \\tModifications and Alternate Forms:\"                                                                                                                         \n [7] \"#' \\tPicheny et al. (2012) use the following logarithmic form of the Goldstein-Price function, on [0, 1]2:\"                                                      \n [8] \"#' \\tThis rescaled form of the function has a mean of zero and a variance of one. The authors also add a small Gaussian error term to the output.\"               \n [9] \"#' @references \\\\url{https://www.sfu.ca/~ssurjano/goldpr.html}\"                                                                                                  ",
        "\n[10] \"#' \\tDixon, L. C. W., & Szego, G. P. (1978). The global optimization problem: an introduction. Towards global optimization, 2, 1-15.\"                            \n[11] \"#' \\tMolga, M., & Smutnicki, C. Test functions for optimization needs (2005). Retrieved June 2013, from http://www.zsd.ict.pwr.wroc.pl/files/docs/functions.pdf.\"\n[12] \"#' \\tPicheny, V., Wagner, T., & Ginsbourger, D. (2012). A benchmark of kriging-based infill criteria for noisy optimization.\"                                    \n[13] \"#' @author Sonja Surjanovic, Simon Fraser University\"                                                                                                            ",
        "\n[14] \"#'          \\tDerek Bingham, Simon Fraser University\"                                                                                                            \n[15] \"#' @param xx c(x1, x2)\"                                                                                                                                          \n[16] \"#' @export goldpr\"                                                                                                                                               \n[17] \"#'\"                                                                                                                                                              ",
        "\n[18] \"\"                                                                                                                                                                \n[19] \"goldpr <- function(xx)\"                                                                                                                                          \n[20] \"{\"                                                                                                                                                               \n[21] \"  \"                                                                                                                                                              ",
        "\n[22] \"  x1 <- xx[1]\"                                                                                                                                                   \n[23] \"  x2 <- xx[2]\"                                                                                                                                                   \n[24] \"\\t\"                                                                                                                                                              \n[25] \"  fact1a <- (x1 + x2 + 1)^2\"                                                                                                                                     ",
        "\n[26] \"  fact1b <- 19 - 14*x1 + 3*x1^2 - 14*x2 + 6*x1*x2 + 3*x2^2\"                                                                                                      \n[27] \"  fact1 <- 1 + fact1a*fact1b\"                                                                                                                                    \n[28] \"\\t\"                                                                                                                                                              \n[29] \"  fact2a <- (2*x1 - 3*x2)^2\"                                                                                                                                     ",
        "\n[30] \"  fact2b <- 18 - 32*x1 + 12*x1^2 + 48*x2 - 36*x1*x2 + 27*x2^2\"                                                                                                   \n[31] \"  fact2 <- 30 + fact2a*fact2b\"                                                                                                                                   \n[32] \"\\t\"                                                                                                                                                              \n[33] \"  y <- fact1*fact2\"                                                                                                                                              ",
        "\n[34] \"  return(y)\"                                                                                                                                                     \n[35] \"}\"                                                                                                                                                               \n\n$`./R/goldprsc.R`\n [1] \"#' GOLDSTEIN-PRICE FUNCTION, SCALED\"                                                                                                                             ",
        "\n [2] \"#'\"                                                                                                                                                              \n [3] \"#' Dimensions: 2\"                                                                                                                                                \n [4] \"#' \\tThe Goldstein-Price function has several local minima.\"                                                                                                     \n [5] \"#' \\tThe function is usually evaluated on the square xi <U+2208> [-2, 2], for all i = 1, 2.\"                                                                     ",
        "\n [6] \"#' \\tModifications and Alternate Forms:\"                                                                                                                         \n [7] \"#' \\tPicheny et al. (2012) use the following logarithmic form of the Goldstein-Price function, on [0, 1]2:\"                                                      \n [8] \"#' \\tThis rescaled form of the function has a mean of zero and a variance of one. The authors also add a small Gaussian error term to the output.\"               \n [9] \"#' @references \\\\url{https://www.sfu.ca/~ssurjano/goldpr.html}\"                                                                                                  ",
        "\n[10] \"#' \\tDixon, L. C. W., & Szego, G. P. (1978). The global optimization problem: an introduction. Towards global optimization, 2, 1-15.\"                            \n[11] \"#' \\tMolga, M., & Smutnicki, C. Test functions for optimization needs (2005). Retrieved June 2013, from http://www.zsd.ict.pwr.wroc.pl/files/docs/functions.pdf.\"\n[12] \"#' \\tPicheny, V., Wagner, T., & Ginsbourger, D. (2012). A benchmark of kriging-based infill criteria for noisy optimization.\"                                    \n[13] \"#' @author Sonja Surjanovic, Simon Fraser University\"                                                                                                            ",
        "\n[14] \"#'          \\tDerek Bingham, Simon Fraser University\"                                                                                                            \n[15] \"#' @param xx c(x1, x2)\"                                                                                                                                          \n[16] \"#' @export goldprsc\"                                                                                                                                             \n[17] \"#'\"                                                                                                                                                              ",
        "\n[18] \"\"                                                                                                                                                                \n[19] \"goldprsc <- function(xx)\"                                                                                                                                        \n[20] \"{\"                                                                                                                                                               \n[21] \"  \"                                                                                                                                                              ",
        "\n[22] \"  x1bar <- 4*xx[1] - 2\"                                                                                                                                          \n[23] \"  x2bar <- 4*xx[2] - 2\"                                                                                                                                          \n[24] \"  fact1a <- (x1bar + x2bar + 1)^2\"                                                                                                                               \n[25] \"  fact1b <- 19 - 14*x1bar + 3*x1bar^2 - 14*x2bar + 6*x1bar*x2bar + 3*x2bar^2\"                                                                                    ",
        "\n[26] \"  fact1 <- 1 + fact1a*fact1b\"                                                                                                                                    \n[27] \"  \"                                                                                                                                                              \n[28] \"  fact2a <- (2*x1bar - 3*x2bar)^2\"                                                                                                                               \n[29] \"  fact2b <- 18 - 32*x1bar + 12*x1bar^2 + 48*x2bar - 36*x1bar*x2bar + 27*x2bar^2\"                                                                                 ",
        "\n[30] \"  fact2 <- 30 + fact2a*fact2b\"                                                                                                                                   \n[31] \"  \"                                                                                                                                                              \n[32] \"  prod <- fact1*fact2\"                                                                                                                                           \n[33] \"  \"                                                                                                                                                              ",
        "\n[34] \"  y <- (log(prod) - 8.693) / 2.427\"                                                                                                                              \n[35] \"  return(y)\"                                                                                                                                                     \n[36] \"}\"                                                                                                                                                               \n\n$`./R/griewank.R`\n",
        " [1] \"#' GRIEWANK FUNCTION\"                                                                                                                                            \n [2] \"#'\"                                                                                                                                                              \n [3] \"#' Dimensions: d\"                                                                                                                                                \n [4] \"#' \\tThe Griewank function has many widespread local minima, which are regularly distributed. The complexity is shown in the zoomed-in plots.\"                   ",
        "\n [5] \"#' \\tThe function is usually evaluated on the hypercube xi <U+2208> [-600, 600], for all i = 1, ?, d.\"                                                           \n [6] \"#' @references \\\\url{https://www.sfu.ca/~ssurjano/griewank.html}\"                                                                                                \n [7] \"#' \\thttp://www-optima.amp.i.kyoto-u.ac.jp/member/student/hedar/Hedar_files/TestGO.htm.\"                                                                         \n [8] \"#' \\tMolga, M., & Smutnicki, C. Test functions for optimization needs (2005). Retrieved June 2013, from http://www.zsd.ict.pwr.wroc.pl/files/docs/functions.pdf.\"",
        "\n [9] \"#' @author Sonja Surjanovic, Simon Fraser University\"                                                                                                            \n[10] \"#'          \\tDerek Bingham, Simon Fraser University\"                                                                                                            \n[11] \"#' @param xx c(x1, x2, ..., xd)\"                                                                                                                                 \n[12] \"#' @export griewank\"                                                                                                                                             ",
        "\n[13] \"#'\"                                                                                                                                                              \n[14] \"\"                                                                                                                                                                \n[15] \"griewank <- function(xx)\"                                                                                                                                        \n[16] \"{\"                                                                                                                                                               ",
        "\n[17] \"\"                                                                                                                                                                \n[18] \"  ii <- c(1:length(xx))\"                                                                                                                                         \n[19] \"  sum <- sum(xx^2/4000)\"                                                                                                                                         \n[20] \"  prod <- prod(cos(xx/sqrt(ii)))\"                                                                                                                                ",
        "\n[21] \"\"                                                                                                                                                                \n[22] \"  y <- sum - prod + 1\"                                                                                                                                           \n[23] \"  return(y)\"                                                                                                                                                     \n[24] \"}\"                                                                                                                                                               ",
        "\n\n$`./R/grlee12.R`\n [1] \"#' GRAMACY & LEE (2012) FUNCTION\"                                                                                                          \n [2] \"#'\"                                                                                                                                        \n [3] \"#' Dimensions: 1\"                                                                                                                          \n [4] \"#' \\tThis is a simple one-dimensional test function.\"                                                                                      ",
        "\n [5] \"#' \\tThis function is usually evaluated on the x <U+2208> [0.5, 2.5].\"                                                                     \n [6] \"#' @references \\\\url{https://www.sfu.ca/~ssurjano/grlee12.html}\"                                                                           \n [7] \"#' \\tGramacy, R. B., & Lee, H. K. (2012). Cases for the nugget in modeling computer experiments. Statistics and Computing, 22(3), 713-722.\"\n [8] \"#' \\tRanjan, P. (2013). Comment: EI Criteria for Noisy Computer Simulators. Technometrics, 55(1), 24-28.\"                                  ",
        "\n [9] \"#' @author Sonja Surjanovic, Simon Fraser University\"                                                                                      \n[10] \"#'          \\tDerek Bingham, Simon Fraser University\"                                                                                      \n[11] \"#' @export grlee12\"                                                                                                                        \n[12] \"#'\"                                                                                                                                        ",
        "\n[13] \"\"                                                                                                                                          \n[14] \"grlee12 <- function(x)\"                                                                                                                    \n[15] \"{\"                                                                                                                                         \n[16] \"  \"                                                                                                                                        ",
        "\n[17] \"  term1 <- sin(10*pi*x) / (2*x)\"                                                                                                           \n[18] \"  term2 <- (x-1)^4\"                                                                                                                        \n[19] \"  \"                                                                                                                                        \n[20] \"  y <- term1 + term2\"                                                                                                                      ",
        "\n[21] \"  return(y)\"                                                                                                                               \n[22] \"}\"                                                                                                                                         \n\n$`./R/hart3.R`\n [1] \"#' HARTMANN 3-DIMENSIONAL FUNCTION\"                                                              \n [2] \"#'\"                                                                                              ",
        "\n [3] \"#' Dimensions: 3\"                                                                                \n [4] \"#' \\tThe 3-dimensional Hartmann function has 4 local minima.\"                                    \n [5] \"#' \\tThe function is usually evaluated on the hypercube xi <U+2208> (0, 1), for all i = 1, 2, 3.\"\n [6] \"#' \\tReference:\"                                                                                 \n [7] \"#' \\thttp://www-optima.amp.i.kyoto-u.ac.jp/member/student/hedar/Hedar_files/TestGO.htm.\"         ",
        "\n [8] \"#' @author Sonja Surjanovic, Simon Fraser University\"                                            \n [9] \"#' \\tDerek Bingham, Simon Fraser University\"                                                     \n[10] \"#' @param xx c(x1, x2, x3)\"                                                                      \n[11] \"#' @export hart3\"                                                                                \n[12] \"#'\"                                                                                              ",
        "\n[13] \"\"                                                                                                \n[14] \"hart3 <- function(xx)\"                                                                           \n[15] \"{\"                                                                                               \n[16] \" \"                                                                                               \n[17] \" alpha <- c(1.0, 1.2, 3.0, 3.2)\"                                                                 ",
        "\n[18] \" A <- c(3.0, 10, 30,\"                                                                            \n[19] \" 0.1, 10, 35,\"                                                                                   \n[20] \" 3.0, 10, 30,\"                                                                                   \n[21] \" 0.1, 10, 35)\"                                                                                   \n[22] \" A <- matrix(A, 4, 3, byrow=TRUE)\"                                                               ",
        "\n[23] \" P <- 10^(-4) * c(3689, 1170, 2673,\"                                                             \n[24] \" 4699, 4387, 7470,\"                                                                              \n[25] \" 1091, 8732, 5547,\"                                                                              \n[26] \" 381, 5743, 8828)\"                                                                               \n[27] \" P <- matrix(P, 4, 3, byrow=TRUE)\"                                                               ",
        "\n[28] \" xxmat <- matrix(rep(xx,times=4), 4, 3, byrow=TRUE)\"                                             \n[29] \" inner <- rowSums(A[,1:3]*(xxmat-P[,1:3])^2)\"                                                    \n[30] \" outer <- sum(alpha * exp(-inner))\"                                                              \n[31] \" y <- -outer\"                                                                                    \n[32] \" return(y)\"                                                                                      ",
        "\n[33] \"}\"                                                                                               \n\n$`./R/hart4.R`\n [1] \"#' HARTMANN 4-DIMENSIONAL FUNCTION\"                                                                                                                                                                                         \n [2] \"#'\"                                                                                                                                                                                                                         ",
        "\n [3] \"#' Dimensions: 4\"                                                                                                                                                                                                           \n [4] \"#' \\tThe 4-dimensional Hartmann function is multimodal. It is given here in the form of Picheny et al. (2012), having a mean of zero and a variance of one. The authors also add a small Gaussian error term to the output.\"\n [5] \"#' \\tThe function is evaluated on the hypercube xi <U+2208> [0, 1], for all i = 1, 2, 3, 4.\"                                                                                                                                ",
        "\n [6] \"#' @references \\\\url{https://www.sfu.ca/~ssurjano/hart4.html}\"                                                                                                                                                              \n [7] \"#' \\tDixon, L. C. W., & Szego, G. P. (1978). The global optimization problem: an introduction. Towards global optimization, 2, 1-15.\"                                                                                       \n [8] \"#' \\tPicheny, V., Wagner, T., & Ginsbourger, D. (2012). A benchmark of kriging-based infill criteria for noisy optimization.\"                                                                                               ",
        "\n [9] \"#' @author Sonja Surjanovic, Simon Fraser University\"                                                                                                                                                                       \n[10] \"#'          \\tDerek Bingham, Simon Fraser University\"                                                                                                                                                                       \n[11] \"#' @param xx c(x1, x2, x3, x4)\"                                                                                                                                                                                             ",
        "\n[12] \"#' @export hart4\"                                                                                                                                                                                                           \n[13] \"#'\"                                                                                                                                                                                                                         \n[14] \"\"                                                                                                                                                                                                                           ",
        "\n[15] \"hart4 <- function(xx)\"                                                                                                                                                                                                      \n[16] \"{\"                                                                                                                                                                                                                          \n[17] \"  \"                                                                                                                                                                                                                         ",
        "\n[18] \"  alpha <- c(1.0, 1.2, 3.0, 3.2)\"                                                                                                                                                                                           \n[19] \"  A <- c(10, 3, 17, 3.5, 1.7, 8,\"                                                                                                                                                                                           \n[20] \"         0.05, 10, 17, 0.1, 8, 14,\"                                                                                                                                                                                         ",
        "\n[21] \"         3, 3.5, 1.7, 10, 17, 8,\"                                                                                                                                                                                           \n[22] \"         17, 8, 0.05, 10, 0.1, 14)\"                                                                                                                                                                                         \n[23] \"  A <- matrix(A, 4, 6, byrow=TRUE)\"                                                                                                                                                                                         ",
        "\n[24] \"  P <- 10^(-4) * c(1312, 1696, 5569, 124, 8283, 5886,\"                                                                                                                                                                      \n[25] \"                   2329, 4135, 8307, 3736, 1004, 9991,\"                                                                                                                                                                     \n[26] \"                   2348, 1451, 3522, 2883, 3047, 6650,\"                                                                                                                                                                     ",
        "\n[27] \"                   4047, 8828, 8732, 5743, 1091, 381)\"                                                                                                                                                                      \n[28] \"  P <- matrix(P, 4, 6, byrow=TRUE)\"                                                                                                                                                                                         \n[29] \"  \"                                                                                                                                                                                                                         ",
        "\n[30] \"  xxmat <- matrix(rep(xx,times=4), 4, 4, byrow=TRUE)\"                                                                                                                                                                       \n[31] \"  inner <- rowSums(A[,1:4]*(xxmat-P[,1:4])^2)\"                                                                                                                                                                              \n[32] \"  outer <- sum(alpha * exp(-inner))\"                                                                                                                                                                                        ",
        "\n[33] \"  \"                                                                                                                                                                                                                         \n[34] \"  y <- (1.1 - outer) / 0.839\"                                                                                                                                                                                               \n[35] \"  return(y)\"                                                                                                                                                                                                                ",
        "\n[36] \"}\"                                                                                                                                                                                                                          \n\n$`./R/hart6.R`\n [1] \"#' HARTMANN 6-DIMENSIONAL FUNCTION\"                                                                                                               \n [2] \"#'\"                                                                                                                                               ",
        "\n [3] \"#' Dimensions: 6\"                                                                                                                                 \n [4] \"#' \\tThe 6-dimensional Hartmann function has 6 local minima.\"                                                                                     \n [5] \"#' \\tThe function is usually evaluated on the hypercube xi <U+2208> (0, 1), for all i = 1, ?, 6.\"                                                 \n [6] \"#' \\tModifications and Alternate Forms:\"                                                                                                          ",
        "\n [7] \"#' \\tPicheny et al. (2012) use the following rescaled form of the 6-dimensional Hartmann function:\"                                               \n [8] \"#' \\tThis rescaled form of the function has a mean of zero and a variance of one. The authors also add a small Gaussian error term to the output.\"\n [9] \"#' @references \\\\url{https://www.sfu.ca/~ssurjano/hart6.html}\"                                                                                    \n[10] \"#' \\tDixon, L. C. W., & Szego, G. P. (1978). The global optimization problem: an introduction. Towards global optimization, 2, 1-15.\"             ",
        "\n[11] \"#' \\thttp://www-optima.amp.i.kyoto-u.ac.jp/member/student/hedar/Hedar_files/TestGO.htm.\"                                                          \n[12] \"#' \\tPicheny, V., Wagner, T., & Ginsbourger, D. (2012). A benchmark of kriging-based infill criteria for noisy optimization.\"                     \n[13] \"#' @author Sonja Surjanovic, Simon Fraser University\"                                                                                             \n[14] \"#'          \\tDerek Bingham, Simon Fraser University\"                                                                                             ",
        "\n[15] \"#' @param xx c(x1, x2, x3, x4, x5, x6)\"                                                                                                           \n[16] \"#' @export hart6\"                                                                                                                                 \n[17] \"#'\"                                                                                                                                               \n[18] \"\"                                                                                                                                                 ",
        "\n[19] \"hart6 <- function(xx)\"                                                                                                                            \n[20] \"{\"                                                                                                                                                \n[21] \"\"                                                                                                                                                 \n[22] \"  alpha <- c(1.0, 1.2, 3.0, 3.2)\"                                                                                                                 ",
        "\n[23] \"  A <- c(10, 3, 17, 3.5, 1.7, 8,\"                                                                                                                 \n[24] \"         0.05, 10, 17, 0.1, 8, 14,\"                                                                                                               \n[25] \"         3, 3.5, 1.7, 10, 17, 8,\"                                                                                                                 \n[26] \"         17, 8, 0.05, 10, 0.1, 14)\"                                                                                                               ",
        "\n[27] \"  A <- matrix(A, 4, 6, byrow=TRUE)\"                                                                                                               \n[28] \"  P <- 10^(-4) * c(1312, 1696, 5569, 124, 8283, 5886,\"                                                                                            \n[29] \"                   2329, 4135, 8307, 3736, 1004, 9991,\"                                                                                           \n[30] \"                   2348, 1451, 3522, 2883, 3047, 6650,\"                                                                                           ",
        "\n[31] \"                   4047, 8828, 8732, 5743, 1091, 381)\"                                                                                            \n[32] \"  P <- matrix(P, 4, 6, byrow=TRUE)\"                                                                                                               \n[33] \"\"                                                                                                                                                 \n[34] \"  xxmat <- matrix(rep(xx,times=4), 4, 6, byrow=TRUE)\"                                                                                             ",
        "\n[35] \"  inner <- rowSums(A[,1:6]*(xxmat-P[,1:6])^2)\"                                                                                                    \n[36] \"  outer <- sum(alpha * exp(-inner))\"                                                                                                              \n[37] \"\"                                                                                                                                                 \n[38] \"  y <- -(2.58 + outer) / 1.94\"                                                                                                                    ",
        "\n[39] \"  return(y)\"                                                                                                                                      \n[40] \"}\"                                                                                                                                                \n\n$`./R/hart6sc.R`\n [1] \"#' HARTMANN 6-DIMENSIONAL FUNCTION, RESCALED\"                                                                                                     \n [2] \"#'\"                                                                                                                                               ",
        "\n [3] \"#' Dimensions: 6\"                                                                                                                                 \n [4] \"#' \\tThe 6-dimensional Hartmann function has 6 local minima.\"                                                                                     \n [5] \"#' \\tThe function is usually evaluated on the hypercube xi <U+2208> (0, 1), for all i = 1, ?, 6.\"                                                 \n [6] \"#' \\tModifications and Alternate Forms:\"                                                                                                          ",
        "\n [7] \"#' \\tPicheny et al. (2012) use the following rescaled form of the 6-dimensional Hartmann function:\"                                               \n [8] \"#' \\tThis rescaled form of the function has a mean of zero and a variance of one. The authors also add a small Gaussian error term to the output.\"\n [9] \"#' @references \\\\url{https://www.sfu.ca/~ssurjano/hart6.html}\"                                                                                    \n[10] \"#' \\tDixon, L. C. W., & Szego, G. P. (1978). The global optimization problem: an introduction. Towards global optimization, 2, 1-15.\"             ",
        "\n[11] \"#' \\thttp://www-optima.amp.i.kyoto-u.ac.jp/member/student/hedar/Hedar_files/TestGO.htm.\"                                                          \n[12] \"#' \\tPicheny, V., Wagner, T., & Ginsbourger, D. (2012). A benchmark of kriging-based infill criteria for noisy optimization.\"                     \n[13] \"#' @author Sonja Surjanovic, Simon Fraser University\"                                                                                             \n[14] \"#'          \\tDerek Bingham, Simon Fraser University\"                                                                                             ",
        "\n[15] \"#' @param xx c(x1, x2, x3, x4, x5, x6)\"                                                                                                           \n[16] \"#' @export hart6sc\"                                                                                                                               \n[17] \"#'\"                                                                                                                                               \n[18] \"\"                                                                                                                                                 ",
        "\n[19] \"hart6sc <- function(xx)\"                                                                                                                          \n[20] \"{\"                                                                                                                                                \n[21] \"\"                                                                                                                                                 \n[22] \"  alpha <- c(1.0, 1.2, 3.0, 3.2)\"                                                                                                                 ",
        "\n[23] \"  A <- c(10, 3, 17, 3.5, 1.7, 8,\"                                                                                                                 \n[24] \"         0.05, 10, 17, 0.1, 8, 14,\"                                                                                                               \n[25] \"         3, 3.5, 1.7, 10, 17, 8,\"                                                                                                                 \n[26] \"         17, 8, 0.05, 10, 0.1, 14)\"                                                                                                               ",
        "\n[27] \"  A <- matrix(A, 4, 6, byrow=TRUE)\"                                                                                                               \n[28] \"  P <- 10^(-4) * c(1312, 1696, 5569, 124, 8283, 5886,\"                                                                                            \n[29] \"                   2329, 4135, 8307, 3736, 1004, 9991,\"                                                                                           \n[30] \"                   2348, 1451, 3522, 2883, 3047, 6650,\"                                                                                           ",
        "\n[31] \"                   4047, 8828, 8732, 5743, 1091, 381)\"                                                                                            \n[32] \"  P <- matrix(P, 4, 6, byrow=TRUE)\"                                                                                                               \n[33] \"\"                                                                                                                                                 \n[34] \"  xxmat <- matrix(rep(xx,times=4), 4, 6, byrow=TRUE)\"                                                                                             ",
        "\n[35] \"  inner <- rowSums(A[,1:6]*(xxmat-P[,1:6])^2)\"                                                                                                    \n[36] \"  outer <- sum(alpha * exp(-inner))\"                                                                                                              \n[37] \"\"                                                                                                                                                 \n[38] \"  y <- -outer\"                                                                                                                                    ",
        "\n[39] \"  return(y)\"                                                                                                                                      \n[40] \"}\"                                                                                                                                                \n\n$`./R/hello.R`\n [1] \"# Hello, world!\"                                              \n [2] \"#\"                                                            \n [3] \"# This is an example function named 'hello' \"                 ",
        "\n [4] \"# which prints 'Hello, world!'.\"                              \n [5] \"#\"                                                            \n [6] \"# You can learn more about package authoring with RStudio at:\"\n [7] \"#\"                                                            \n [8] \"#   http://r-pkgs.had.co.nz/\"                                 \n [9] \"#\"                                                            \n[10] \"# Some useful keyboard shortcuts for package authoring:\"      \n[11] \"#\"                                                            ",
        "\n[12] \"#   Install Package:           'Ctrl + Shift + B'\"            \n[13] \"#   Check Package:             'Ctrl + Shift + E'\"            \n[14] \"#   Test Package:              'Ctrl + Shift + T'\"            \n[15] \"\"                                                             \n[16] \"hello <- function() {\"                                        \n[17] \"  print(\\\"Hello, world!\\\")\"                                   \n[18] \"}\"                                                            \n\n$`./R/holder.R`\n [1] \"#' HOLDER TABLE FUNCTION\"                                                                                                                    ",
        "\n [2] \"#'\"                                                                                                                                          \n [3] \"#' Dimensions: 2\"                                                                                                                            \n [4] \"#' \\tThe Holder Table function has many local minima, with four global minima.\"                                                              \n [5] \"#' \\tThe function is usually evaluated on the square xi <U+2208> [-10, 10], for all i = 1, 2.\"                                               ",
        "\n [6] \"#' @references \\\\url{https://www.sfu.ca/~ssurjano/holder.html}\"                                                                              \n [7] \"#' \\tTest functions for optimization. In Wikipedia. Retrieved June 2013, from https://en.wikipedia.org/wiki/Test_functions_for_optimization.\"\n [8] \"#' @author Sonja Surjanovic, Simon Fraser University\"                                                                                        \n [9] \"#'          \\tDerek Bingham, Simon Fraser University\"                                                                                        ",
        "\n[10] \"#' @param xx c(x1, x2)\"                                                                                                                      \n[11] \"#' @export holder\"                                                                                                                           \n[12] \"#'\"                                                                                                                                          \n[13] \"\"                                                                                                                                            ",
        "\n[14] \"holder <- function(xx)\"                                                                                                                      \n[15] \"{\"                                                                                                                                           \n[16] \"  \"                                                                                                                                          \n[17] \"  x1 <- xx[1]\"                                                                                                                               ",
        "\n[18] \"  x2 <- xx[2]\"                                                                                                                               \n[19] \"\\t\"                                                                                                                                          \n[20] \"  fact1 <- sin(x1)*cos(x2)\"                                                                                                                  \n[21] \"  fact2 <- exp(abs(1 - sqrt(x1^2+x2^2)/pi))\"                                                                                                 ",
        "\n[22] \"\\t\"                                                                                                                                          \n[23] \"  y <- -abs(fact1*fact2)\"                                                                                                                    \n[24] \"  return(y)\"                                                                                                                                 \n[25] \"}\"                                                                                                                                           ",
        "\n\n$`./R/langer.R`\n [1] \"#' LANGERMANN FUNCTION\"                                                                                                                                                                                              \n [2] \"#'\"                                                                                                                                                                                                                  \n [3] \"#' Dimensions: d\"                                                                                                                                                                                                    ",
        "\n [4] \"#' \\tThe Langermann function is multimodal, with many unevenly distributed local minima. The recommended values of m, c and A, as given by Molga & Smutnicki (2005) are (for d = 2): m = 5, c = (1, 2, 5, 2, 3) and:\"\n [5] \"#' \\tThe function is usually evaluated on the hypercube xi <U+2208> [0, 10], for all i = 1, ?, d.\"                                                                                                                   \n [6] \"#' \\tReference:\"                                                                                                                                                                                                     ",
        "\n [7] \"#' \\tAdorio, E. P., & Diliman, U. P. MVF - Multivariate Test Functions Library in C for Unconstrained Global Optimization (2005). Retrieved June 2013, from http://http://www.geocities.ws/eadorio/mvf.pdf.\"         \n [8] \"#' \\tMolga, M., & Smutnicki, C. Test functions for optimization needs (2005). Retrieved June 2013, from http://www.zsd.ict.pwr.wroc.pl/files/docs/functions.pdf.\"                                                    \n [9] \"#' @author Sonja Surjanovic, Simon Fraser University\"                                                                                                                                                                ",
        "\n[10] \"#'          \\tDerek Bingham, Simon Fraser University\"                                                                                                                                                                \n[11] \"#' \\txx   = c(x1, x2, ..., xd)\"                                                                                                                                                                                      \n[12] \"#' \\tm    = constant (optional), with default value 5\"                                                                                                                                                               ",
        "\n[13] \"#' @param cvec m-dimensional vector (optional), with default value c(1, 2, 5, 2, 3)\"                                                                                                                                 \n[14] \"#' \\tA    = (mxd)-dimensional matrix (optional), with default value:\"                                                                                                                                                \n[15] \"#' @export langer\"                                                                                                                                                                                                   ",
        "\n[16] \"#'\"                                                                                                                                                                                                                  \n[17] \"\"                                                                                                                                                                                                                    \n[18] \"langer <- function(xx, m=5, cvec, A)\"                                                                                                                                                                                ",
        "\n[19] \"{\"                                                                                                                                                                                                                   \n[20] \"\"                                                                                                                                                                                                                    \n[21] \"  d <- length(xx)\"                                                                                                                                                                                                   ",
        "\n[22] \"\"                                                                                                                                                                                                                    \n[23] \"  if (missing(cvec)) {\"                                                                                                                                                                                              \n[24] \"    if (m == 5){\"                                                                                                                                                                                                    ",
        "\n[25] \"      cvec <- c(1,2,5,2,3)\"                                                                                                                                                                                          \n[26] \"    }\"                                                                                                                                                                                                               \n[27] \"    else {\"                                                                                                                                                                                                          ",
        "\n[28] \"      stop('Value of the m-dimensional vector cvec is required.')\"                                                                                                                                                   \n[29] \"    }\"                                                                                                                                                                                                               \n[30] \"  }\"                                                                                                                                                                                                                 ",
        "\n[31] \"\"                                                                                                                                                                                                                    \n[32] \"  if (missing(A)) {\"                                                                                                                                                                                                 \n[33] \"    if (m==5 && d==2) {\"                                                                                                                                                                                             ",
        "\n[34] \"      A <- matrix(c(3,5,5,2,2,1,1,4,7,9),5,2,byrow=TRUE)\"                                                                                                                                                            \n[35] \"    }\"                                                                                                                                                                                                               \n[36] \"    else {\"                                                                                                                                                                                                          ",
        "\n[37] \"        stop('Value of the (mxd)-dimensional matrix A is required.')\"                                                                                                                                                \n[38] \"    }\"                                                                                                                                                                                                               \n[39] \"  }\"                                                                                                                                                                                                                 ",
        "\n[40] \"\"                                                                                                                                                                                                                    \n[41] \"  xxmat <- matrix(rep(xx,times=m), m, d, byrow=TRUE)\"                                                                                                                                                                \n[42] \"  inner <- rowSums((xxmat-A[,1:d])^2)\"                                                                                                                                                                               ",
        "\n[43] \"  outer <- sum(cvec * exp(-inner/pi) * cos(pi*inner))\"                                                                                                                                                               \n[44] \"\"                                                                                                                                                                                                                    \n[45] \"  y <- outer\"                                                                                                                                                                                                        ",
        "\n[46] \"  return(y)\"                                                                                                                                                                                                         \n[47] \"}\"                                                                                                                                                                                                                   \n\n$`./R/levy.R`\n [1] \"#' LEVY FUNCTION\"                                                                                                                                                                                                      ",
        "\n [2] \"#'\"                                                                                                                                                                                                                    \n [3] \"#' Dimensions: d\"                                                                                                                                                                                                      \n [4] \"#' \\tThe function is usually evaluated on the hypercube xi <U+2208> [-10, 10], for all i = 1, ?, d.\"                                                                                                                   ",
        "\n [5] \"#' \\tReference:\"                                                                                                                                                                                                       \n [6] \"#' \\tLaguna, M., & Marti, R. Experimental Testing of Advanced Scatter Search Designs for Global Optimization of Multimodal Functions (2002). Retrieved June 2013, from http://www.uv.es/rmarti/paper/docs/global1.pdf.\"\n [7] \"#' @author Sonja Surjanovic, Simon Fraser University\"                                                                                                                                                                  ",
        "\n [8] \"#'          \\tDerek Bingham, Simon Fraser University\"                                                                                                                                                                  \n [9] \"#' @param xx c(x1, x2, ..., xd)\"                                                                                                                                                                                       \n[10] \"#' @export levy\"                                                                                                                                                                                                       ",
        "\n[11] \"#'\"                                                                                                                                                                                                                    \n[12] \"\"                                                                                                                                                                                                                      \n[13] \"levy <- function(xx)\"                                                                                                                                                                                                  ",
        "\n[14] \"{\"                                                                                                                                                                                                                     \n[15] \"\"                                                                                                                                                                                                                      \n[16] \"  d <- length(xx)\"                                                                                                                                                                                                     ",
        "\n[17] \"  w <- 1 + (xx - 1)/4\"                                                                                                                                                                                                 \n[18] \"\"                                                                                                                                                                                                                      \n[19] \"  term1 <- (sin(pi*w[1]))^2\"                                                                                                                                                                                           ",
        "\n[20] \"  term3 <- (w[d]-1)^2 * (1+1*(sin(2*pi*w[d]))^2)\"                                                                                                                                                                      \n[21] \"\"                                                                                                                                                                                                                      \n[22] \"  wi <- w[1:(d-1)]\"                                                                                                                                                                                                    ",
        "\n[23] \"  sum <- sum((wi-1)^2 * (1+10*(sin(pi*wi+1))^2))\"                                                                                                                                                                      \n[24] \"\"                                                                                                                                                                                                                      \n[25] \"  y <- term1 + sum + term3\"                                                                                                                                                                                            ",
        "\n[26] \"  return(y)\"                                                                                                                                                                                                           \n[27] \"}\"                                                                                                                                                                                                                     \n\n$`./R/levy13.R`\n [1] \"#' LEVY FUNCTION N. 13\"                                                                       ",
        "\n [2] \"#'\"                                                                                           \n [3] \"#' Dimensions: 2\"                                                                             \n [4] \"#' \\tThe function is usually evaluated on the square xi <U+2208> [-10, 10], for all i = 1, 2.\"\n [5] \"#' \\tReference:\"                                                                              \n [6] \"#' @author Sonja Surjanovic, Simon Fraser University\"                                         \n [7] \"#'          \\tDerek Bingham, Simon Fraser University\"                                         ",
        "\n [8] \"#' @param xx c(x1, x2)\"                                                                       \n [9] \"#' @export levy13\"                                                                            \n[10] \"#'\"                                                                                           \n[11] \"\"                                                                                             \n[12] \"levy13 <- function(xx)\"                                                                       \n[13] \"{\"                                                                                            ",
        "\n[14] \"  \"                                                                                           \n[15] \"  x1 <- xx[1]\"                                                                                \n[16] \"  x2 <- xx[2]\"                                                                                \n[17] \"\\t\"                                                                                           \n[18] \"  term1 <- (sin(3*pi*x1))^2\"                                                                  \n[19] \"  term2 <- (x1-1)^2 * (1+(sin(3*pi*x2))^2)\"                                                   ",
        "\n[20] \"  term3 <- (x2-1)^2 * (1+(sin(2*pi*x2))^2)\"                                                   \n[21] \"\\t\"                                                                                           \n[22] \"  y <- term1 + term2 + term3\"                                                                 \n[23] \"  return(y)\"                                                                                  \n[24] \"}\"                                                                                            \n\n$`./R/main.R`\n",
        " [1] \"# method_compare('ackley', p = 2, budget = 100)->a\" \n [2] \"# save(a, file = \\\"a.rda\\\")\"                        \n [3] \"# rm(a)\"                                            \n [4] \"# method_compare('ackley', p = 6, budget = 150)->b\" \n [5] \"# save(b, file = \\\"b.rda\\\")\"                        \n [6] \"# rm(b)\"                                            \n [7] \"# method_compare('ackley', p = 10, budget = 200)->c\"\n [8] \"# save(c, file = \\\"c.rda\\\")\"                        \n [9] \"# rm(c)\"                                            ",
        "\n[10] \"# method_compare('beale', budget = 100)->d\"         \n[11] \"# save(d, file = \\\"d.rda\\\")\"                        \n[12] \"# rm(d)\"                                            \n[13] \"# method_compare('boha1', budget = 100)->e\"         \n[14] \"# save(e, file = \\\"e.rda\\\")\"                        \n[15] \"# rm(e)\"                                            \n[16] \"# method_compare('boha2', budget = 100)->f\"         \n[17] \"# save(f, file = \\\"f.rda\\\")\"                        \n[18] \"# rm(f)\"                                            ",
        "\n[19] \"# method_compare('booth', budget = 100)->g\"         \n[20] \"# save(g, file = \\\"g.rda\\\")\"                        \n[21] \"# rm(g)\"                                            \n[22] \"# method_compare('branin', budget = 100)->h\"        \n[23] \"# save(h, file = \\\"h.rda\\\")\"                        \n[24] \"# rm(h)\"                                            \n[25] \"# method_compare('braninmodif', budget = 100)->i\"   \n[26] \"# save(i, file = \\\"i.rda\\\")\"                        \n[27] \"# rm(c)\"                                            ",
        "\n[28] \"\"                                                   \n[29] \"\"                                                   \n[30] \"# method_compare('bukin6', budget = 100)->j\"        \n[31] \"# method_compare('camel3', budget = 100)->k\"        \n[32] \"# method_compare('camel6', budget = 100)->l\"        \n[33] \"# method_compare('colville', budget = 100)->m\"      \n[34] \"# method_compare('crossit', budget = 100)->n\"       \n[35] \"# method_compare('dejong5', budget = 100)->o\"       \n[36] \"# method_compare('dixonpr', budget = 100)->p\"       ",
        "\n[37] \"# method_compare('easom', budget = 100)->q\"         \n[38] \"\"                                                   \n[39] \"# save(j,k,l,m,n,o,p,q, file = \\\"j_q.rda\\\")\"        \n[40] \"# rm(j,k,l,m,n,o,p,q)\"                              \n[41] \"\"                                                   \n[42] \"# method_compare('goldpr', budget = 100)->r\"        \n[43] \"# method_compare('goldprsc', budget = 100)->s\"      \n[44] \"# method_compare('hart3', budget = 100)->t\"         \n[45] \"# method_compare('hart4', budget = 120)->u\"         ",
        "\n[46] \"# method_compare('hart6', budget = 150)->v\"         \n[47] \"# method_compare('powell', p=10, budget = 200)->w\"  \n[48] \"# method_compare('schaffer2', budget = 100)->x\"     \n[49] \"# method_compare('schaffer4', budget = 100)->y\"     \n[50] \"# method_compare('shekel', budget = 100)->z \"       \n[51] \"\"                                                   \n[52] \"# save(r,s,t,u,v,w,x,y,z, file = 'r_z.rda')\"        \n[53] \"# rm(r,s,t,u,v,w,x,y,z)#\"                           \n\n$`./R/matya.R`\n [1] \"#' MATYAS FUNCTION\"                                                                           ",
        "\n [2] \"#'\"                                                                                           \n [3] \"#' Dimensions: 2\"                                                                             \n [4] \"#' \\tThe Matyas function has no local minima except the global one.\"                          \n [5] \"#' \\tThe function is usually evaluated on the square xi <U+2208> [-10, 10], for all i = 1, 2.\"\n [6] \"#' \\tReference:\"                                                                              \n [7] \"#' \\thttp://www-optima.amp.i.kyoto-u.ac.jp/member/student/hedar/Hedar_files/TestGO.htm.\"      ",
        "\n [8] \"#' @author Sonja Surjanovic, Simon Fraser University\"                                         \n [9] \"#'          \\tDerek Bingham, Simon Fraser University\"                                         \n[10] \"#' @param xx c(x1, x2)\"                                                                       \n[11] \"#' @export matya\"                                                                             \n[12] \"#'\"                                                                                           \n[13] \"\"                                                                                             ",
        "\n[14] \"matya <- function(xx)\"                                                                        \n[15] \"{\"                                                                                            \n[16] \"  \"                                                                                           \n[17] \"  x1 <- xx[1]\"                                                                                \n[18] \"  x2 <- xx[2]\"                                                                                \n[19] \"\\t\"                                                                                           ",
        "\n[20] \"  term1 <- 0.26 * (x1^2 + x2^2)\"                                                              \n[21] \"  term2 <- -0.48*x1*x2\"                                                                       \n[22] \"\\t\"                                                                                           \n[23] \"  y <- term1 + term2\"                                                                         \n[24] \"  return(y)\"                                                                                  \n[25] \"}\"                                                                                            ",
        "\n\n$`./R/mccorm.R`\n [1] \"#' MCCORMICK FUNCTION\"                                                                                                                                                                                      \n [2] \"#'\"                                                                                                                                                                                                         \n [3] \"#' Dimensions: 2\"                                                                                                                                                                                           ",
        "\n [4] \"#' \\tThe function is usually evaluated on the rectangle x1 <U+2208> [-1.5, 4], x2 <U+2208> [-3, 4].\"                                                                                                        \n [5] \"#' \\tReference:\"                                                                                                                                                                                            \n [6] \"#' \\tAdorio, E. P., & Diliman, U. P. MVF - Multivariate Test Functions Library in C for Unconstrained Global Optimization (2005). Retrieved June 2013, from http://http://www.geocities.ws/eadorio/mvf.pdf.\"",
        "\n [7] \"#' @author Sonja Surjanovic, Simon Fraser University\"                                                                                                                                                       \n [8] \"#'          \\tDerek Bingham, Simon Fraser University\"                                                                                                                                                       \n [9] \"#' @param xx c(x1, x2)\"                                                                                                                                                                                     ",
        "\n[10] \"#' @export mccorm\"                                                                                                                                                                                          \n[11] \"#'\"                                                                                                                                                                                                         \n[12] \"\"                                                                                                                                                                                                           ",
        "\n[13] \"mccorm <- function(xx)\"                                                                                                                                                                                     \n[14] \"{\"                                                                                                                                                                                                          \n[15] \"  \"                                                                                                                                                                                                         ",
        "\n[16] \"  x1 <- xx[1]\"                                                                                                                                                                                              \n[17] \"  x2 <- xx[2]\"                                                                                                                                                                                              \n[18] \"\\t\"                                                                                                                                                                                                         ",
        "\n[19] \"  term1 <- sin(x1 + x2)\"                                                                                                                                                                                    \n[20] \"  term2 <-(x1 - x2)^2\"                                                                                                                                                                                      \n[21] \"  term3 <- -1.5*x1\"                                                                                                                                                                                         ",
        "\n[22] \"  term4 <- 2.5*x2\"                                                                                                                                                                                          \n[23] \"\\t\"                                                                                                                                                                                                         \n[24] \"  y <- term1 + term2 + term3 + term4 + 1\"                                                                                                                                                                   ",
        "\n[25] \"  return(y)\"                                                                                                                                                                                                \n[26] \"}\"                                                                                                                                                                                                          \n\n$`./R/michal.R`\n [1] \"#' MICHALEWICZ FUNCTION\"                                                                                                                                                                                                                                                                      ",
        "\n [2] \"#'\"                                                                                                                                                                                                                                                                                           \n [3] \"#' Dimensions: d\"                                                                                                                                                                                                                                                                             ",
        "\n [4] \"#' \\tThe Michalewicz function has d! local minima, and it is multimodal. The parameter m defines the steepness of they valleys and ridges; a larger m leads to a more difficult search. The recommended value of m is m = 10. The function's two-dimensional form is shown in the plot above.\"\n [5] \"#' \\tThe function is usually evaluated on the hypercube xi <U+2208> [0, p], for all i = 1, ?, d.\"                                                                                                                                                                                             ",
        "\n [6] \"#' @references \\\\url{https://www.sfu.ca/~ssurjano/michal.html}\"                                                                                                                                                                                                                               \n [7] \"#' \\thttp://www-optima.amp.i.kyoto-u.ac.jp/member/student/hedar/Hedar_files/TestGO.htm.\"                                                                                                                                                                                                      ",
        "\n [8] \"#' \\tMolga, M., & Smutnicki, C. Test functions for optimization needs (2005). Retrieved June 2013, from http://www.zsd.ict.pwr.wroc.pl/files/docs/functions.pdf.\"                                                                                                                             \n [9] \"#' @author Sonja Surjanovic, Simon Fraser University\"                                                                                                                                                                                                                                         ",
        "\n[10] \"#'          \\tDerek Bingham, Simon Fraser University\"                                                                                                                                                                                                                                         \n[11] \"#' @param xx c(x1, x2)\"                                                                                                                                                                                                                                                                       ",
        "\n[12] \"#' @param m constant (optional), with default value 10\"                                                                                                                                                                                                                                       \n[13] \"#' @export michal\"                                                                                                                                                                                                                                                                            ",
        "\n[14] \"#'\"                                                                                                                                                                                                                                                                                           \n[15] \"\"                                                                                                                                                                                                                                                                                             ",
        "\n[16] \"michal <- function(xx, m=10)\"                                                                                                                                                                                                                                                                 \n[17] \"{\"                                                                                                                                                                                                                                                                                            ",
        "\n[18] \"\"                                                                                                                                                                                                                                                                                             \n[19] \"  ii <- c(1:length(xx))\"                                                                                                                                                                                                                                                                      ",
        "\n[20] \"  sum <- sum(sin(xx) * (sin(ii*xx^2/pi))^(2*m))\"                                                                                                                                                                                                                                              \n[21] \"\"                                                                                                                                                                                                                                                                                             ",
        "\n[22] \"  y <- -sum\"                                                                                                                                                                                                                                                                                  \n[23] \"  return(y)\"                                                                                                                                                                                                                                                                                  ",
        "\n[24] \"}\"                                                                                                                                                                                                                                                                                            \n\n$`./R/optimize_fun.R`\n  [1] \"#' Optimize fun\"                                                                                          \n  [2] \"#'\"                                                                                                       ",
        "\n  [3] \"#' Uses EGO algorithm to optimize any given function.\"                                                    \n  [4] \"#'\"                                                                                                       \n  [5] \"#' @author BLANK\"                                                                                         \n  [6] \"#'\"                                                                                                       \n  [7] \"#'\"                                                                                                       ",
        "\n  [8] \"#' @export\"                                                                                               \n  [9] \"\"                                                                                                         \n [10] \"optimize_fun <- function(fun, lower, upper, ..., X = NULL, y = NULL,\"                                     \n [11] \"                         tolerr = 1e-4, maxit = 20,\"                                                      \n [12] \"                         nsteps = 5, rho = 0.3, trueglobal = NULL, maximize = FALSE,\"                     ",
        "\n [13] \"                         do_maxit = FALSE, counter = 5, dimplot = 1:2, budget = NULL,\"                    \n [14] \"                         basicEGO = FALSE, n = 5*length(lower), plot = FALSE, trace = 1,\"                 \n [15] \"                         method = c('fastEGO','TREGO'),\"                                                  \n [16] \"                         increament_rate = 0,\"                                                            \n [17] \"                         seed = NULL, cost = NULL){\"                                                      ",
        "\n [18] \"\"                                                                                                         \n [19] \"  # Rewrite the function in case of maximization:\"                                                        \n [20] \"  .fun <-  function(x) (-1)^(maximize)*fun(x, ...)\"                                                       \n [21] \"  if(!is.null(trueglobal)) trueglobal <- (-1)^maximize*trueglobal\"                                        \n [22] \"  # Initialization\"                                                                                       ",
        "\n [23] \"  count <- 0\"                                                                                             \n [24] \"  signifs <- ceiling(1-log10(tolerr))\"                                                                    \n [25] \"  init_lower <- lower\"                                                                                    \n [26] \"  init_upper <- upper\"                                                                                    \n [27] \"  method <- getFromNamespace(paste0(method[1], \\\".nsteps\\\"), \\\"DiceOptim\\\")\"                              ",
        "\n [28] \"\"                                                                                                         \n [29] \"\"                                                                                                         \n [30] \"  # Generate scaled X matrix and Y values\"                                                                \n [31] \"  p <- length(lower)\"                                                                                     \n [32] \"\"                                                                                                         ",
        "\n [33] \"  if(!is.null(budget)) {\"                                                                                 \n [34] \"    add <- budget - n\"                                                                                    \n [35] \"    n <- n + add %% nsteps\"                                                                               \n [36] \"    maxit <- add %/% nsteps\"                                                                              \n [37] \"    do_maxit <- TRUE\"                                                                                     ",
        "\n [38] \"  }\"                                                                                                      \n [39] \"  errors <- numeric(maxit)\"                                                                               \n [40] \"  set.seed(seed)\"                                                                                         \n [41] \"  if(is.null(X)) {\"                                                                                       \n [42] \"    X <- lhs::maximinLHS(n, p)\"                                                                           ",
        "\n [43] \"    X <- mapply(scales::rescale, data.frame(X),data.frame(rbind(lower, upper)))\"                          \n [44] \"    y <- apply(X, 1, .fun) #\"                                                                             \n [45] \"  }\"                                                                                                      \n [46] \"\"                                                                                                         \n [47] \"\"                                                                                                         ",
        "\n [48] \"  # Starting Optimal value -- Enable to compute error.\"                                                   \n [49] \"  optimal <- min(y)\"                                                                                      \n [50] \"  error_init <- if(!is.null(trueglobal)) {\"                                                               \n [51] \"    maximize + optimal - trueglobal*(!maximize)\"                                                          \n [52] \"  }else NULL\"                                                                                             ",
        "\n [53] \"\"                                                                                                         \n [54] \"  if(!is.function(cost)) cost <- .fun\"                                                                    \n [55] \"  loss_init <- cost(X[which.min(y),])\"                                                                    \n [56] \"  # Run kriging model\"                                                                                    \n [57] \"  model <- DiceKriging::km(design = X, response = y, control = list(trace = 0))\"                          ",
        "\n [58] \"\"                                                                                                         \n [59] \"\"                                                                                                         \n [60] \"  #plot the first 2 dimensions holding the other dimensions at midpoint:\"                                 \n [61] \"  if(plot){\"                                                                                              \n [62] \"    use_colors <- c(\\\"red\\\", \\\"blue\\\", \\\"green\\\",\\\"yellow\\\", \\\"orange\\\",\\\"purple\\\",\"                      ",
        "\n [63] \"                    rainbow(maxit))\"                                                                      \n [64] \"    if(length(dimplot)!=2) stop(\\\"you can only plot 2d\\\", call. = FALSE)\"                                 \n [65] \"    col_order <- order(c(dimplot, setdiff(seq_len(p), dimplot)))\"                                         \n [66] \"\"                                                                                                         \n [67] \"    rem <- if (p>2) tail((lower + upper)/2, -2) else NULL\"                                                ",
        "\n [68] \"    x1 <- seq(lower[dimplot[1]], upper[dimplot[1]], length = 50)\"                                         \n [69] \"    x2 <- seq(lower[dimplot[2]], upper[dimplot[2]], length = 50)\"                                         \n [70] \"    Z <- outer(x1, x2, Vectorize(\\\\(x, y).fun(c(x, y, rem)[col_order])))\"                                 \n [71] \"    my_contour(x1, x2, Z)\"                                                                                \n [72] \"    points(cbind(X, y), pch=16, col=1)\"                                                                   ",
        "\n [73] \"    rect(lower[dimplot[1]], lower[dimplot[2]],\"                                                           \n [74] \"         upper[dimplot[1]], upper[dimplot[2]], border = 'black')\"                                         \n [75] \"  }\"                                                                                                      \n [76] \"  loss <- NULL\"                                                                                           \n [77] \"  for(i in seq_len(maxit)){\"                                                                              ",
        "\n [78] \"\"                                                                                                         \n [79] \"    #Fit the EGO\"                                                                                         \n [80] \"    oEGO <- method(model, .fun, nsteps, lower, upper,trace = FALSE)\"                                      \n [81] \"    model <- oEGO$lastmodel\"                                                                              \n [82] \"\"                                                                                                         ",
        "\n [83] \"    # Obtain the center and ROI\"                                                                          \n [84] \"    o <- order(model@y)\"                                                                                  \n [85] \"    center <- model@X[o[1], ]\"                                                                            \n [86] \"\"                                                                                                         \n [87] \"    # Compute the error and set the previous optimal to be the current optimal\"                           ",
        "\n [88] \"    err <- optimal - model@y[o[1]]\"                                                                       \n [89] \"    optimal <- model@y[o[1]]\"                                                                             \n [90] \"    if(is.function(cost)) loss[i] <- cost(center)\"                                                        \n [91] \"\"                                                                                                         \n [92] \"    if(!basicEGO) {\"                                                                                      ",
        "\n [93] \"      top_n <- ceiling(rho * nrow(model@X))\"                                                              \n [94] \"      dist <- diff(apply( model@X[o[seq_len(top_n)], ], 2, range))/2\"                                     \n [95] \"      lower <- c(pmax(center - dist, init_lower))\"                                                        \n [96] \"      upper <- c(pmin(center + dist, init_upper))\"                                                        \n [97] \"      if(err < tolerr) {\"                                                                                 ",
        "\n [98] \"        count <- count + 1\"                                                                               \n [99] \"        if(increament_rate > 0 ){\"                                                                        \n[100] \"          lower <- c(pmax(center - (1 + increament_rate)^(1/p)*dist, init_lower))\"                        \n[101] \"          upper <- c(pmin(center + (1 + increament_rate)^(1/p)*dist, init_upper))\"                        \n[102] \"        }\"                                                                                                ",
        "\n[103] \"        else {\"                                                                                           \n[104] \"          lower <- init_lower\"                                                                            \n[105] \"          upper <- init_upper\"                                                                            \n[106] \"        }\"                                                                                                \n[107] \"      }\"                                                                                                  ",
        "\n[108] \"    }\"                                                                                                    \n[109] \"\"                                                                                                         \n[110] \"    #Plot the new ROI and the added points\"                                                               \n[111] \"    if(plot){\"                                                                                            \n[112] \"      rect(lower[dimplot[1]], lower[dimplot[2]],\"                                                         ",
        "\n[113] \"           upper[dimplot[1]], upper[dimplot[2]], border = use_colors[i])\"                                 \n[114] \"      points(oEGO$par[,dimplot, drop = FALSE], col=use_colors[i], pch=16)\"                                \n[115] \"      Sys.sleep(1)\"                                                                                       \n[116] \"    }\"                                                                                                    \n[117] \"    if(!is.null(trueglobal)) {\"                                                                           ",
        "\n[118] \"      trueERR <- optimal - trueglobal\"                                                                    \n[119] \"      errors[i] <- trueERR\"                                                                               \n[120] \"    }\"                                                                                                    \n[121] \"\"                                                                                                         \n[122] \"    if(trace){\"                                                                                           ",
        "\n[123] \"      cat(sprintf(sprintf(\\\"it: %02d\\\\tf(x*): %%.%df\\\\terrr: %%.%df\\\\t\\\", i, signifs, signifs),\"          \n[124] \"                  optimal*(-1)^(maximize),err))\"                                                          \n[125] \"      if(p<6){\"                                                                                           \n[126] \"        fmt <- sprintf(\\\"[%s]\\\\tcount:%02i\\\\t\\\", trimws(strrep(\\\"%.4f, \\\", p),'r',\\\", \\\"), count)\"        \n[127] \"        cat(do.call(sprintf, c(fmt, as.list(center))))\"                                                   ",
        "\n[128] \"      }\"                                                                                                  \n[129] \"      if(!is.null(trueglobal))cat(sprintf(sprintf(\\\"trueERR: %%.%df\\\", signifs), trueERR))\"               \n[130] \"      cat(\\\"\\\\n\\\")\"                                                                                       \n[131] \"    }\"                                                                                                    \n[132] \"\"                                                                                                         ",
        "\n[133] \"\"                                                                                                         \n[134] \"    if((count >= counter  || !is.null(trueglobal) && trueERR < tolerr) && !do_maxit) break\"               \n[135] \"    #cat(\\\"lower:[\\\", lower,\\\"] upper:[\\\", upper,\\\"]\\\\n\\\")\"                                               \n[136] \"\"                                                                                                         \n[137] \"  }\"                                                                                                      ",
        "\n[138] \"  structure(list(par = unname(center),\"                                                                   \n[139] \"                 value = optimal * (-1)^(maximize),\"                                                      \n[140] \"                 model = model,\"                                                                          \n[141] \"                 env = environment(),\"                                                                    \n[142] \"                 errors = c(error_init, errors),\"                                                         ",
        "\n[143] \"                 loss = c(loss_init, loss)),\"                                                             \n[144] \"            class = 'egoOptim')\"                                                                          \n[145] \"}\"                                                                                                        \n[146] \"#' print.egoOptim\"                                                                                        \n[147] \"#' print an egoOptim object\"                                                                              ",
        "\n[148] \"#' @param x an object of class egoOptim\"                                                                  \n[149] \"#' @export\"                                                                                               \n[150] \"print.egoOptim <- function(x,...){\"                                                                       \n[151] \"  cat(\\\"\\\\nCall:\\\\n\\\", paste(deparse(x$call), sep = \\\"\\\\n\\\", collapse = \\\"\\\\n\\\"), \\\"\\\\n\\\\n\\\", sep = \\\"\\\")\"\n[152] \"  cat(\\\"\\\\t\\\\t\\\\t\\\\tKriging Based RSO\\\\n\\\")\"                                                              ",
        "\n[153] \"  cat(strrep(\\\"=\\\", 73), \\\"\\\\n\\\")\"                                                                        \n[154] \"  cat(do.call(sprintf,c(sprintf(\\\"\\\\tx*=[%s]\\\", trimws(strrep(\\\"%.4f, \\\", x$model@d),'r',\\\", \\\")),\"       \n[155] \"                        as.list(x$par))))\"                                                                \n[156] \"  cat(sprintf(\\\"\\\\tf(x*) = %.4f\\\\t\\\\t\\\", x$value))\"                                                       \n[157] \"  cat(\\\"Total Points:\\\", x$model@n, \\\"\\\\n\\\")\"                                                             ",
        "\n[158] \"  cat(strrep(\\\"_\\\", 73), \\\"\\\\n\\\")\"                                                                        \n[159] \"}\"                                                                                                        \n[160] \"\"                                                                                                         \n\n$`./R/perm0db.R`\n [1] \"#' PERM FUNCTION 0, d, beta\"                                                                      \n [2] \"#'\"                                                                                               ",
        "\n [3] \"#' Dimensions: d\"                                                                                 \n [4] \"#' \\tThe function is usually evaluated on the hypercube xi <U+2208> [-d, d], for all i = 1, ?, d.\"\n [5] \"#' \\tReference:\"                                                                                  \n [6] \"#' \\thttp://www-optima.amp.i.kyoto-u.ac.jp/member/student/hedar/Hedar_files/TestGO.htm.\"          \n [7] \"#' @author Sonja Surjanovic, Simon Fraser University\"                                             ",
        "\n [8] \"#'          \\tDerek Bingham, Simon Fraser University\"                                             \n [9] \"#' @param xx c(x1, x2)\"                                                                           \n[10] \"#' @param b constant (optional), with default value 10\"                                           \n[11] \"#' @export perm0db\"                                                                               \n[12] \"#'\"                                                                                               ",
        "\n[13] \"\"                                                                                                 \n[14] \"perm0db <- function(xx, b=10)\"                                                                    \n[15] \"{\"                                                                                                \n[16] \"\"                                                                                                 \n[17] \"  d <- length(xx)\"                                                                                ",
        "\n[18] \"  ii <- c(1:d)\"                                                                                   \n[19] \"  jj <- matrix(rep(ii,times=d), d, d, byrow=TRUE)\"                                                \n[20] \"  xxmat <- matrix(rep(xx,times=d), d, d, byrow=TRUE)\"                                             \n[21] \"  inner <- rowSums((jj+b)*(xxmat^ii-(1/jj)^ii))\"                                                  \n[22] \"  outer <- sum(inner^2)\"                                                                          ",
        "\n[23] \"\"                                                                                                 \n[24] \"  y <- outer\"                                                                                     \n[25] \"  return(y)\"                                                                                      \n[26] \"}\"                                                                                                \n\n$`./R/permdb.R`\n [1] \"#' PERM FUNCTION d, beta\"                                                                         ",
        "\n [2] \"#'\"                                                                                               \n [3] \"#' Dimensions: d\"                                                                                 \n [4] \"#' \\tThe Perm d, ? function.\"                                                                     \n [5] \"#' \\tThe function is usually evaluated on the hypercube xi <U+2208> [-d, d], for all i = 1, ?, d.\"\n [6] \"#' \\tReference:\"                                                                                  ",
        "\n [7] \"#' \\thttp://www-optima.amp.i.kyoto-u.ac.jp/member/student/hedar/Hedar_files/TestGO.htm.\"          \n [8] \"#' @author Sonja Surjanovic, Simon Fraser University\"                                             \n [9] \"#'          \\tDerek Bingham, Simon Fraser University\"                                             \n[10] \"#' @param xx c(x1, x2)\"                                                                           \n[11] \"#' \\tb  = constant (optional), with default value 0.5\"                                            ",
        "\n[12] \"#' @export permdb\"                                                                                \n[13] \"#'\"                                                                                               \n[14] \"\"                                                                                                 \n[15] \"permdb <- function(xx, b=0.5)\"                                                                    \n[16] \"{\"                                                                                                ",
        "\n[17] \"\"                                                                                                 \n[18] \"  d <- length(xx)\"                                                                                \n[19] \"  ii <- c(1:d)\"                                                                                   \n[20] \"  jj <- matrix(rep(ii,times=d), d, d, byrow=TRUE)\"                                                \n[21] \"  xxmat <- matrix(rep(xx,times=d), d, d, byrow=TRUE)\"                                             ",
        "\n[22] \"  inner <- rowSums((jj^ii+b)*((xxmat/jj)^ii-1))\"                                                  \n[23] \"  outer <- sum(inner^2)\"                                                                          \n[24] \"\"                                                                                                 \n[25] \"  y <- outer\"                                                                                     \n[26] \"  return(y)\"                                                                                      ",
        "\n[27] \"}\"                                                                                                \n\n$`./R/powell.R`\n [1] \"#' POWELL FUNCTION\"                                                                                                                                                                                                    \n [2] \"#'\"                                                                                                                                                                                                                    ",
        "\n [3] \"#' Dimensions: d\"                                                                                                                                                                                                      \n [4] \"#' \\tThe function is usually evaluated on the hypercube xi <U+2208> [-4, 5], for all i = 1, ?, d.\"                                                                                                                     \n [5] \"#' \\tReference:\"                                                                                                                                                                                                       ",
        "\n [6] \"#' \\thttp://www-optima.amp.i.kyoto-u.ac.jp/member/student/hedar/Hedar_files/TestGO.htm.\"                                                                                                                               \n [7] \"#' \\tLaguna, M., & Marti, R. Experimental Testing of Advanced Scatter Search Designs for Global Optimization of Multimodal Functions (2002). Retrieved June 2013, from http://www.uv.es/rmarti/paper/docs/global1.pdf.\"\n [8] \"#' @author Sonja Surjanovic, Simon Fraser University\"                                                                                                                                                                  ",
        "\n [9] \"#'          \\tDerek Bingham, Simon Fraser University\"                                                                                                                                                                  \n[10] \"#' @param xx c(x1, x2, ..., xd)\"                                                                                                                                                                                       \n[11] \"#' @export powell\"                                                                                                                                                                                                     ",
        "\n[12] \"#'\"                                                                                                                                                                                                                    \n[13] \"\"                                                                                                                                                                                                                      \n[14] \"powell <- function(xx)\"                                                                                                                                                                                                ",
        "\n[15] \"{\"                                                                                                                                                                                                                     \n[16] \"\"                                                                                                                                                                                                                      \n[17] \"  d <- length(xx)\"                                                                                                                                                                                                     ",
        "\n[18] \"\"                                                                                                                                                                                                                      \n[19] \"  xxa <- xx[seq(1, d-3, 4)]\"                                                                                                                                                                                           \n[20] \"  xxb <- xx[seq(2, d-2, 4)]\"                                                                                                                                                                                           ",
        "\n[21] \"  xxc <- xx[seq(3, d-1, 4)]\"                                                                                                                                                                                           \n[22] \"  xxd <- xx[seq(4, d, 4)]\"                                                                                                                                                                                             \n[23] \"  sumterm1 <- (xxa + 10*xxb)^2\"                                                                                                                                                                                        ",
        "\n[24] \"  sumterm2 <- 5 * (xxc - xxd)^2\"                                                                                                                                                                                       \n[25] \"  sumterm3 <- (xxb - 2*xxc)^4\"                                                                                                                                                                                         \n[26] \"  sumterm4 <- 10 * (xxa - xxd)^4\"                                                                                                                                                                                      ",
        "\n[27] \"  sum <- sum(sumterm1 + sumterm2 + sumterm3 + sumterm4)\"                                                                                                                                                               \n[28] \"\"                                                                                                                                                                                                                      \n[29] \"  y <- sum\"                                                                                                                                                                                                            ",
        "\n[30] \"  return(y)\"                                                                                                                                                                                                           \n[31] \"}\"                                                                                                                                                                                                                     \n\n$`./R/powersum.R`\n [1] \"#' POWER SUM FUNCTION\"                                                                                                                                  ",
        "\n [2] \"#'\"                                                                                                                                                     \n [3] \"#' Dimensions: d\"                                                                                                                                       \n [4] \"#' \\tThe Power Sum function. It is shown above in its two-dimensional form. The recommended value of the b-vector, for d = 4, is: b = (8, 18, 44, 114).\"\n [5] \"#' \\tThe function is usually evaluated on the hypercube xi <U+2208> [0, d], for all i = 1, ?, d.\"                                                       ",
        "\n [6] \"#' \\tReference:\"                                                                                                                                        \n [7] \"#' \\thttp://www-optima.amp.i.kyoto-u.ac.jp/member/student/hedar/Hedar_files/TestGO.htm.\"                                                                \n [8] \"#' @author Sonja Surjanovic, Simon Fraser University\"                                                                                                   \n [9] \"#'          \\tDerek Bingham, Simon Fraser University\"                                                                                                   ",
        "\n[10] \"#' @param xx c(x1, x2)\"                                                                                                                                 \n[11] \"#' \\tb  = d-dimensional vector (optional), with default value\"                                                                                          \n[12] \"#'      \\tc(8, 18, 44, 114) (when d=4)\"                                                                                                                 \n[13] \"#' @export powersum\"                                                                                                                                    ",
        "\n[14] \"#'\"                                                                                                                                                     \n[15] \"\"                                                                                                                                                       \n[16] \"powersum <- function(xx, b)\"                                                                                                                            \n[17] \"{\"                                                                                                                                                      ",
        "\n[18] \"\"                                                                                                                                                       \n[19] \"  d <- length(xx)\"                                                                                                                                      \n[20] \"  ii <- c(1:d)\"                                                                                                                                         \n[21] \"\"                                                                                                                                                       ",
        "\n[22] \"  if (missing(b)) {\"                                                                                                                                    \n[23] \"    if (d == 4){\"                                                                                                                                       \n[24] \"      b <- c(8, 18, 44, 114)\"                                                                                                                           \n[25] \"    }\"                                                                                                                                                  ",
        "\n[26] \"    else {\"                                                                                                                                             \n[27] \"      stop('Value of the d-dimensional vector b is required.')\"                                                                                         \n[28] \"    }\"                                                                                                                                                  \n[29] \"  }\"                                                                                                                                                    ",
        "\n[30] \"\"                                                                                                                                                       \n[31] \"  xxmat <- matrix(rep(xx,times=d), d, d, byrow=TRUE)\"                                                                                                   \n[32] \"  inner <- rowSums(xxmat^ii)\"                                                                                                                           \n[33] \"  outer <- sum((inner-b)^2)\"                                                                                                                            ",
        "\n[34] \"\"                                                                                                                                                       \n[35] \"  y <- outer\"                                                                                                                                           \n[36] \"  return(y)\"                                                                                                                                            \n[37] \"}\"                                                                                                                                                      ",
        "\n\n$`./R/rastr.R`\n [1] \"#' RASTRIGIN FUNCTION\"                                                                                                                                                                           \n [2] \"#'\"                                                                                                                                                                                              \n [3] \"#' Dimensions: d\"                                                                                                                                                                                ",
        "\n [4] \"#' \\tThe Rastrigin function has several local minima. It is highly multimodal, but locations of the minima are regularly distributed. It is shown in the plot above in its two-dimensional form.\"\n [5] \"#' \\tThe function is usually evaluated on the hypercube xi <U+2208> [-5.12, 5.12], for all i = 1, ?, d.\"                                                                                         \n [6] \"#' @references \\\\url{https://www.sfu.ca/~ssurjano/rastr.html}\"                                                                                                                                   ",
        "\n [7] \"#' \\thttp://www-optima.amp.i.kyoto-u.ac.jp/member/student/hedar/Hedar_files/TestGO.htm.\"                                                                                                         \n [8] \"#' \\tPohlheim, H. GEATbx Examples: Examples of Objective Functions (2005). Retrieved June 2013, from http://www.geatbx.com/download/GEATbx_ObjFunExpl_v37.pdf.\"                                  \n [9] \"#' @author Sonja Surjanovic, Simon Fraser University\"                                                                                                                                            ",
        "\n[10] \"#'          \\tDerek Bingham, Simon Fraser University\"                                                                                                                                            \n[11] \"#' @param xx c(x1, x2, ..., xd)\"                                                                                                                                                                 \n[12] \"#' @export rastr\"                                                                                                                                                                                ",
        "\n[13] \"#'\"                                                                                                                                                                                              \n[14] \"\"                                                                                                                                                                                                \n[15] \"rastr <- function(xx)\"                                                                                                                                                                           ",
        "\n[16] \"{\"                                                                                                                                                                                               \n[17] \"\"                                                                                                                                                                                                \n[18] \"  d <- length(xx)\"                                                                                                                                                                               ",
        "\n[19] \"\"                                                                                                                                                                                                \n[20] \"  sum <- sum(xx^2 - 10*cos(2*pi*xx))\"                                                                                                                                                            \n[21] \"\"                                                                                                                                                                                                ",
        "\n[22] \"  y <- 10*d + sum\"                                                                                                                                                                               \n[23] \"  return(y)\"                                                                                                                                                                                     \n[24] \"}\"                                                                                                                                                                                               ",
        "\n\n$`./R/rosen.R`\n [1] \"#' ROSENBROCK FUNCTION\"                                                                                                                                                                                           \n [2] \"#'\"                                                                                                                                                                                                               \n [3] \"#' Dimensions: d\"                                                                                                                                                                                                 ",
        "\n [4] \"#' \\tThe Rosenbrock function, also referred to as the Valley or Banana function, is a popular test problem for gradient-based optimization algorithms. It is shown in the plot above in its two-dimensional form.\"\n [5] \"#' \\tThe function is unimodal, and the global minimum lies in a narrow, parabolic valley. However, even though this valley is easy to find, convergence to the minimum is difficult (Picheny et al., 2012).\"      \n [6] \"#' \\tThe function is usually evaluated on the hypercube xi <U+2208> [-5, 10], for all i = 1, ?, d, although it may be restricted to the hypercube xi <U+2208> [-2.048, 2.048], for all i = 1, ?, d.\"              ",
        "\n [7] \"#' \\tModifications and Alternate Forms:\"                                                                                                                                                                          \n [8] \"#' \\tPicheny et al. (2012) use the following rescaled form of the function, with d = 4, on [0, 1]2:\"                                                                                                              \n [9] \"#' \\tThis rescaled form of the function has a mean of zero and a variance of one. The authors also add a small Gaussian error term to the output.\"                                                                ",
        "\n[10] \"#' @references \\\\url{https://www.sfu.ca/~ssurjano/rosen.html}\"                                                                                                                                                    \n[11] \"#' \\tDixon, L. C. W., & Szego, G. P. (1978). The global optimization problem: an introduction. Towards global optimization, 2, 1-15.\"                                                                             \n[12] \"#' \\thttp://www-optima.amp.i.kyoto-u.ac.jp/member/student/hedar/Hedar_files/TestGO.htm.\"                                                                                                                          ",
        "\n[13] \"#' \\tMolga, M., & Smutnicki, C. Test functions for optimization needs (2005). Retrieved June 2013, from http://www.zsd.ict.pwr.wroc.pl/files/docs/functions.pdf.\"                                                 \n[14] \"#' \\tPicheny, V., Wagner, T., & Ginsbourger, D. (2012). A benchmark of kriging-based infill criteria for noisy optimization.\"                                                                                     \n[15] \"#' @author Sonja Surjanovic, Simon Fraser University\"                                                                                                                                                             ",
        "\n[16] \"#'          \\tDerek Bingham, Simon Fraser University\"                                                                                                                                                             \n[17] \"#' @param xx c(x1, x2, ..., xd)\"                                                                                                                                                                                  \n[18] \"#' @export rosen\"                                                                                                                                                                                                 ",
        "\n[19] \"#'\"                                                                                                                                                                                                               \n[20] \"\"                                                                                                                                                                                                                 \n[21] \"rosen <- function(xx)\"                                                                                                                                                                                            ",
        "\n[22] \"{\"                                                                                                                                                                                                                \n[23] \"\"                                                                                                                                                                                                                 \n[24] \"  d <- length(xx)\"                                                                                                                                                                                                ",
        "\n[25] \"  xi <- xx[1:(d-1)]\"                                                                                                                                                                                              \n[26] \"  xnext <- xx[2:d]\"                                                                                                                                                                                               \n[27] \"\"                                                                                                                                                                                                                 ",
        "\n[28] \"  sum <- sum(100*(xnext-xi^2)^2 + (xi-1)^2)\"                                                                                                                                                                      \n[29] \"\"                                                                                                                                                                                                                 \n[30] \"  y <- sum\"                                                                                                                                                                                                       ",
        "\n[31] \"  return(y)\"                                                                                                                                                                                                      \n[32] \"}\"                                                                                                                                                                                                                \n\n$`./R/rosensc.R`\n [1] \"#' ROSENBROCK FUNCTION, RESCALED\"                                                                                                                                                                                 ",
        "\n [2] \"#'\"                                                                                                                                                                                                               \n [3] \"#' Dimensions: d\"                                                                                                                                                                                                 \n [4] \"#' \\tThe Rosenbrock function, also referred to as the Valley or Banana function, is a popular test problem for gradient-based optimization algorithms. It is shown in the plot above in its two-dimensional form.\"",
        "\n [5] \"#' \\tThe function is unimodal, and the global minimum lies in a narrow, parabolic valley. However, even though this valley is easy to find, convergence to the minimum is difficult (Picheny et al., 2012).\"      \n [6] \"#' \\tThe function is usually evaluated on the hypercube xi <U+2208> [-5, 10], for all i = 1, ?, d, although it may be restricted to the hypercube xi <U+2208> [-2.048, 2.048], for all i = 1, ?, d.\"              \n [7] \"#' \\tModifications and Alternate Forms:\"                                                                                                                                                                          ",
        "\n [8] \"#' \\tPicheny et al. (2012) use the following rescaled form of the function, with d = 4, on [0, 1]2:\"                                                                                                              \n [9] \"#' \\tThis rescaled form of the function has a mean of zero and a variance of one. The authors also add a small Gaussian error term to the output.\"                                                                \n[10] \"#' @references \\\\url{https://www.sfu.ca/~ssurjano/rosen.html}\"                                                                                                                                                    ",
        "\n[11] \"#' \\tDixon, L. C. W., & Szego, G. P. (1978). The global optimization problem: an introduction. Towards global optimization, 2, 1-15.\"                                                                             \n[12] \"#' \\thttp://www-optima.amp.i.kyoto-u.ac.jp/member/student/hedar/Hedar_files/TestGO.htm.\"                                                                                                                          \n[13] \"#' \\tMolga, M., & Smutnicki, C. Test functions for optimization needs (2005). Retrieved June 2013, from http://www.zsd.ict.pwr.wroc.pl/files/docs/functions.pdf.\"                                                 ",
        "\n[14] \"#' \\tPicheny, V., Wagner, T., & Ginsbourger, D. (2012). A benchmark of kriging-based infill criteria for noisy optimization.\"                                                                                     \n[15] \"#' @author Sonja Surjanovic, Simon Fraser University\"                                                                                                                                                             \n[16] \"#'          \\tDerek Bingham, Simon Fraser University\"                                                                                                                                                             ",
        "\n[17] \"#' @param xx c(x1, x2, ..., xd)\"                                                                                                                                                                                  \n[18] \"#' @export rosensc\"                                                                                                                                                                                               \n[19] \"#'\"                                                                                                                                                                                                               ",
        "\n[20] \"\"                                                                                                                                                                                                                 \n[21] \"rosensc <- function(xx)\"                                                                                                                                                                                          \n[22] \"{\"                                                                                                                                                                                                                ",
        "\n[23] \"\"                                                                                                                                                                                                                 \n[24] \"  xxbar <- 15*xx - 5\"                                                                                                                                                                                             \n[25] \"  xibar <- xxbar[1:3]\"                                                                                                                                                                                            ",
        "\n[26] \"  xnextbar <- xxbar[2:4]\"                                                                                                                                                                                         \n[27] \"\"                                                                                                                                                                                                                 \n[28] \"  sum <- sum(100*(xnextbar-xibar^2)^2 + (1 - xibar)^2)\"                                                                                                                                                           ",
        "\n[29] \"\"                                                                                                                                                                                                                 \n[30] \"  y <- (sum - 3.827*10^5) / (3.755*10^5)\"                                                                                                                                                                         \n[31] \"  return(y)\"                                                                                                                                                                                                      ",
        "\n[32] \"}\"                                                                                                                                                                                                                \n\n$`./R/rothyp.R`\n [1] \"#' ROTATED HYPER-ELLIPSOID FUNCTION\"                                                                                                                                                                                                   \n [2] \"#'\"                                                                                                                                                                                                                                    ",
        "\n [3] \"#' Dimensions: d\"                                                                                                                                                                                                                      \n [4] \"#' \\tThe Rotated Hyper-Ellipsoid function is continuous, convex and unimodal. It is an extension of the Axis Parallel Hyper-Ellipsoid function, also referred to as the Sum Squares function. The plot shows its two-dimensional form.\"\n [5] \"#' \\tThe function is usually evaluated on the hypercube xi <U+2208> [-65.536, 65.536], for all i = 1, ?, d.\"                                                                                                                           ",
        "\n [6] \"#' \\tReference:\"                                                                                                                                                                                                                       \n [7] \"#' \\tMolga, M., & Smutnicki, C. Test functions for optimization needs (2005). Retrieved June 2013, from http://www.zsd.ict.pwr.wroc.pl/files/docs/functions.pdf.\"                                                                      \n [8] \"#' @author Sonja Surjanovic, Simon Fraser University\"                                                                                                                                                                                  ",
        "\n [9] \"#'          \\tDerek Bingham, Simon Fraser University\"                                                                                                                                                                                  \n[10] \"#' @param xx c(x1, x2, ..., xd)\"                                                                                                                                                                                                       \n[11] \"#' @export rothyp\"                                                                                                                                                                                                                     ",
        "\n[12] \"#'\"                                                                                                                                                                                                                                    \n[13] \"\"                                                                                                                                                                                                                                      \n[14] \"rothyp <- function(xx)\"                                                                                                                                                                                                                ",
        "\n[15] \"{\"                                                                                                                                                                                                                                     \n[16] \"\"                                                                                                                                                                                                                                      \n[17] \"  d <- length(xx)\"                                                                                                                                                                                                                     ",
        "\n[18] \"  xxmat <- matrix(rep(xx,times=d), d, d, byrow=TRUE)\"                                                                                                                                                                                  \n[19] \"  xxmatlow <- xxmat\"                                                                                                                                                                                                                   \n[20] \"  xxmatlow[upper.tri(xxmatlow)] <- 0\"                                                                                                                                                                                                  ",
        "\n[21] \"\"                                                                                                                                                                                                                                      \n[22] \"  inner <- rowSums(xxmatlow^2)\"                                                                                                                                                                                                        \n[23] \"  outer <- sum(inner)\"                                                                                                                                                                                                                 ",
        "\n[24] \"\"                                                                                                                                                                                                                                      \n[25] \"  y <- outer\"                                                                                                                                                                                                                          \n[26] \"  return(y)\"                                                                                                                                                                                                                           ",
        "\n[27] \"}\"                                                                                                                                                                                                                                     \n\n$`./R/schaffer2.R`\n [1] \"#' SCHAFFER FUNCTION N. 2\"                                                                                                                   \n [2] \"#'\"                                                                                                                                          ",
        "\n [3] \"#' Dimensions: 2\"                                                                                                                            \n [4] \"#' \\tThe second Schaffer function. It is shown on a smaller input domain in the second plot to show detail.\"                                 \n [5] \"#' \\tThe function is usually evaluated on the square xi <U+2208> [-100, 100], for all i = 1, 2.\"                                             \n [6] \"#' \\tReference:\"                                                                                                                             ",
        "\n [7] \"#' \\tTest functions for optimization. In Wikipedia. Retrieved June 2013, from https://en.wikipedia.org/wiki/Test_functions_for_optimization.\"\n [8] \"#' @author Sonja Surjanovic, Simon Fraser University\"                                                                                        \n [9] \"#'          \\tDerek Bingham, Simon Fraser University\"                                                                                        \n[10] \"#' @param xx c(x1, x2)\"                                                                                                                      ",
        "\n[11] \"#' @export schaffer2\"                                                                                                                        \n[12] \"#'\"                                                                                                                                          \n[13] \"\"                                                                                                                                            \n[14] \"schaffer2 <- function(xx)\"                                                                                                                   ",
        "\n[15] \"{\"                                                                                                                                           \n[16] \"  \"                                                                                                                                          \n[17] \"  x1 <- xx[1]\"                                                                                                                               \n[18] \"  x2 <- xx[2]\"                                                                                                                               ",
        "\n[19] \"\\t\"                                                                                                                                          \n[20] \"  fact1 <- (sin(x1^2-x2^2))^2 - 0.5\"                                                                                                         \n[21] \"  fact2 <- (1 + 0.001*(x1^2+x2^2))^2\"                                                                                                        \n[22] \"\\t\"                                                                                                                                          ",
        "\n[23] \"  y <- 0.5 + fact1/fact2\"                                                                                                                    \n[24] \"  return(y)\"                                                                                                                                 \n[25] \"}\"                                                                                                                                           \n\n$`./R/schaffer4.R`\n [1] \"#' SCHAFFER FUNCTION N. 4\"                                                                                                                   ",
        "\n [2] \"#'\"                                                                                                                                          \n [3] \"#' Dimensions: 2\"                                                                                                                            \n [4] \"#' \\tThe fourth Schaffer function. It is shown on a smaller input domain in the second plot to show detail.\"                                 \n [5] \"#' \\tThe function is usually evaluated on the square xi <U+2208> [-100, 100], for all i = 1, 2.\"                                             ",
        "\n [6] \"#' \\tReference:\"                                                                                                                             \n [7] \"#' \\tTest functions for optimization. In Wikipedia. Retrieved June 2013, from https://en.wikipedia.org/wiki/Test_functions_for_optimization.\"\n [8] \"#' @author Sonja Surjanovic, Simon Fraser University\"                                                                                        \n [9] \"#'          \\tDerek Bingham, Simon Fraser University\"                                                                                        ",
        "\n[10] \"#' @param xx c(x1, x2)\"                                                                                                                      \n[11] \"#' @export schaffer4\"                                                                                                                        \n[12] \"#'\"                                                                                                                                          \n[13] \"\"                                                                                                                                            ",
        "\n[14] \"schaffer4 <- function(xx)\"                                                                                                                   \n[15] \"{\"                                                                                                                                           \n[16] \"  \"                                                                                                                                          \n[17] \"  x1 <- xx[1]\"                                                                                                                               ",
        "\n[18] \"  x2 <- xx[2]\"                                                                                                                               \n[19] \"\\t\"                                                                                                                                          \n[20] \"  fact1 <- (cos(sin(abs(x1^2-x2^2))))^2 - 0.5\"                                                                                               \n[21] \"  fact2 <- (1 + 0.001*(x1^2+x2^2))^2\"                                                                                                        ",
        "\n[22] \"\\t\"                                                                                                                                          \n[23] \"  y <- 0.5 + fact1/fact2\"                                                                                                                    \n[24] \"  return(y)\"                                                                                                                                 \n[25] \"}\"                                                                                                                                           ",
        "\n\n$`./R/schwef.R`\n [1] \"#' SCHWEFEL FUNCTION\"                                                                                                                                                                                                  \n [2] \"#'\"                                                                                                                                                                                                                    \n [3] \"#' Dimensions: d\"                                                                                                                                                                                                      ",
        "\n [4] \"#' \\tThe Schwefel function is complex, with many local minima. The plot shows the two-dimensional form of the function.\"                                                                                               \n [5] \"#' \\tThe function is usually evaluated on the hypercube xi <U+2208> [-500, 500], for all i = 1, ?, d.\"                                                                                                                 \n [6] \"#' \\tReference:\"                                                                                                                                                                                                       ",
        "\n [7] \"#' \\tGEATbx: Examples of Objective Functions. Retrieved September 2014, from http://www.pg.gda.pl/~mkwies/dyd/geadocu/fcnfun7.html.\"                                                                                   \n [8] \"#' \\thttp://www-optima.amp.i.kyoto-u.ac.jp/member/student/hedar/Hedar_files/TestGO.htm.\"                                                                                                                               \n [9] \"#' \\tLaguna, M., & Marti, R. Experimental Testing of Advanced Scatter Search Designs for Global Optimization of Multimodal Functions (2002). Retrieved June 2013, from http://www.uv.es/rmarti/paper/docs/global1.pdf.\"",
        "\n[10] \"#' @author Sonja Surjanovic, Simon Fraser University\"                                                                                                                                                                  \n[11] \"#'          \\tDerek Bingham, Simon Fraser University\"                                                                                                                                                                  \n[12] \"#' @param xx c(x1, x2, ..., xd)\"                                                                                                                                                                                       ",
        "\n[13] \"#' @export schwef\"                                                                                                                                                                                                     \n[14] \"#'\"                                                                                                                                                                                                                    \n[15] \"\"                                                                                                                                                                                                                      ",
        "\n[16] \"schwef <- function(xx)\"                                                                                                                                                                                                \n[17] \"{\"                                                                                                                                                                                                                     \n[18] \"\"                                                                                                                                                                                                                      ",
        "\n[19] \"  d <- length(xx)\"                                                                                                                                                                                                     \n[20] \"\"                                                                                                                                                                                                                      \n[21] \"  sum <- sum(xx*sin(sqrt(abs(xx))))\"                                                                                                                                                                                   ",
        "\n[22] \"  y <- 418.9829*d - sum\"                                                                                                                                                                                               \n[23] \"  return(y)\"                                                                                                                                                                                                           \n[24] \"}\"                                                                                                                                                                                                                     ",
        "\n\n$`./R/shekel.R`\n [1] \"#' SHEKEL FUNCTION\"                                                                                                                                                                    \n [2] \"#'\"                                                                                                                                                                                    \n [3] \"#' Dimensions: 4\"                                                                                                                                                                      ",
        "\n [4] \"#' \\tThe Shekel function has m local minima. Above are the recommended values of m, the ?-vector and the C-matrix; ? is an m-dimensional vector, and C is a 4-by-m-dimensional matrix.\"\n [5] \"#' \\tThe function is usually evaluated on the hypercube xi <U+2208> [0, 10], for all i = 1, 2, 3, 4.\"                                                                                  \n [6] \"#' \\tReference:\"                                                                                                                                                                       ",
        "\n [7] \"#' \\thttp://www-optima.amp.i.kyoto-u.ac.jp/member/student/hedar/Hedar_files/TestGO.htm.\"                                                                                               \n [8] \"#' \\tMolga, M., & Smutnicki, C. Test functions for optimization needs (2005). Retrieved June 2013, from http://www.zsd.ict.pwr.wroc.pl/files/docs/functions.pdf.\"                      \n [9] \"#' @author Sonja Surjanovic, Simon Fraser University\"                                                                                                                                  ",
        "\n[10] \"#'          \\tDerek Bingham, Simon Fraser University\"                                                                                                                                  \n[11] \"#' @param xx c(x1, x2, x3, x4)\"                                                                                                                                                        \n[12] \"#' @export shekel\"                                                                                                                                                                     ",
        "\n[13] \"#'\"                                                                                                                                                                                    \n[14] \"\"                                                                                                                                                                                      \n[15] \"shekel <- function(xx, m=10)\"                                                                                                                                                          ",
        "\n[16] \"{\"                                                                                                                                                                                     \n[17] \"\"                                                                                                                                                                                      \n[18] \"  m <- 10\"                                                                                                                                                                             ",
        "\n[19] \"  b <- 0.1 * c(1, 2, 2, 4, 4, 6, 3, 7, 5, 5)\"                                                                                                                                          \n[20] \"  C <- c(4.0, 1.0, 8.0, 6.0, 3.0, 2.0, 5.0, 8.0, 6.0, 7.0,\"                                                                                                                            \n[21] \"         4.0, 1.0, 8.0, 6.0, 7.0, 9.0, 3.0, 1.0, 2.0, 3.6,\"                                                                                                                            ",
        "\n[22] \"         4.0, 1.0, 8.0, 6.0, 3.0, 2.0, 5.0, 8.0, 6.0, 7.0,\"                                                                                                                            \n[23] \"         4.0, 1.0, 8.0, 6.0, 7.0, 9.0, 3.0, 1.0, 2.0, 3.6)\"                                                                                                                            \n[24] \"  C <- matrix(C, 4, 10, byrow=TRUE)\"                                                                                                                                                   ",
        "\n[25] \"  Ct <- t(C)\"                                                                                                                                                                          \n[26] \"\"                                                                                                                                                                                      \n[27] \"  xxmat <- matrix(rep(xx,times=m), m, 4, byrow=TRUE)\"                                                                                                                                  ",
        "\n[28] \"  inner <- rowSums((xxmat-Ct[,1:4])^2)\"                                                                                                                                                \n[29] \"\"                                                                                                                                                                                      \n[30] \"  outer <- sum(1/(inner+b))\"                                                                                                                                                           ",
        "\n[31] \"\"                                                                                                                                                                                      \n[32] \"  y <- -outer\"                                                                                                                                                                         \n[33] \"  return(y)\"                                                                                                                                                                           ",
        "\n[34] \"}\"                                                                                                                                                                                     \n\n$`./R/shubert.R`\n [1] \"#' SHUBERT FUNCTION\"                                                                                                                                                                     \n [2] \"#'\"                                                                                                                                                                                      ",
        "\n [3] \"#' Dimensions: 2\"                                                                                                                                                                        \n [4] \"#' \\tThe Shubert function has several local minima and many global minima. The second plot shows the the function on a smaller input domain, to allow for easier viewing.\"               \n [5] \"#' \\tThe function is usually evaluated on the square xi <U+2208> [-10, 10], for all i = 1, 2, although this may be restricted to the square xi <U+2208> [-5.12, 5.12], for all i = 1, 2.\"",
        "\n [6] \"#' \\tReference:\"                                                                                                                                                                         \n [7] \"#' \\thttp://www-optima.amp.i.kyoto-u.ac.jp/member/student/hedar/Hedar_files/TestGO.htm.\"                                                                                                 \n [8] \"#' @author Sonja Surjanovic, Simon Fraser University\"                                                                                                                                    ",
        "\n [9] \"#'          \\tDerek Bingham, Simon Fraser University\"                                                                                                                                    \n[10] \"#' @param xx c(x1, x2)\"                                                                                                                                                                  \n[11] \"#' @export shubert\"                                                                                                                                                                      ",
        "\n[12] \"#'\"                                                                                                                                                                                      \n[13] \"\"                                                                                                                                                                                        \n[14] \"shubert <- function(xx)\"                                                                                                                                                                 ",
        "\n[15] \"{\"                                                                                                                                                                                       \n[16] \"  \"                                                                                                                                                                                      \n[17] \"  x1 <- xx[1]\"                                                                                                                                                                           ",
        "\n[18] \"  x2 <- xx[2]\"                                                                                                                                                                           \n[19] \"  ii <- c(1:5)\"                                                                                                                                                                          \n[20] \"\\t\"                                                                                                                                                                                      ",
        "\n[21] \"  sum1 <- sum(ii * cos((ii+1)*x1+ii))\"                                                                                                                                                   \n[22] \"  sum2 <- sum(ii * cos((ii+1)*x2+ii))\"                                                                                                                                                   \n[23] \"\\t\"                                                                                                                                                                                      ",
        "\n[24] \"  y <- sum1 * sum2\"                                                                                                                                                                      \n[25] \"  return(y)\"                                                                                                                                                                             \n[26] \"}\"                                                                                                                                                                                       ",
        "\n\n$`./R/spheref.R`\n [1] \"#' SPHERE FUNCTION\"                                                                                                                                              \n [2] \"#'\"                                                                                                                                                              \n [3] \"#' Dimensions: d\"                                                                                                                                                ",
        "\n [4] \"#' \\tThe Sphere function has d local minima except for the global one. It is continuous, convex and unimodal. The plot shows its two-dimensional form.\"          \n [5] \"#' \\tThe function is usually evaluated on the hypercube xi <U+2208> [-5.12, 5.12], for all i = 1, ?, d.\"                                                         \n [6] \"#' \\tModifications and Alternate Forms:\"                                                                                                                         \n [7] \"#' \\tPicheny et al. (2012) use the following, slightly different, version of the Sphere function, with d = 6, on [0, 1]6:\"                                       ",
        "\n [8] \"#' \\tThis function has a mean of zero and a variance of one. The authors also add a small Gaussian error term to the output.\"                                    \n [9] \"#' @references \\\\url{https://www.sfu.ca/~ssurjano/spheref.html}\"                                                                                                 \n[10] \"#' \\tDixon, L. C. W., & Szego, G. P. (1978). The global optimization problem: an introduction. Towards global optimization, 2, 1-15.\"                            \n[11] \"#' \\tMolga, M., & Smutnicki, C. Test functions for optimization needs (2005). Retrieved June 2013, from http://www.zsd.ict.pwr.wroc.pl/files/docs/functions.pdf.\"",
        "\n[12] \"#' \\tPicheny, V., Wagner, T., & Ginsbourger, D. (2012). A benchmark of kriging-based infill criteria for noisy optimization.\"                                    \n[13] \"#' @author Sonja Surjanovic, Simon Fraser University\"                                                                                                            \n[14] \"#'          \\tDerek Bingham, Simon Fraser University\"                                                                                                            \n[15] \"#' @param xx c(x1, x2, ..., xd)\"                                                                                                                                 ",
        "\n[16] \"#' @export spheref\"                                                                                                                                              \n[17] \"#'\"                                                                                                                                                              \n[18] \"\"                                                                                                                                                                \n[19] \"spheref <- function(xx)\"                                                                                                                                         ",
        "\n[20] \"{\"                                                                                                                                                               \n[21] \"\"                                                                                                                                                                \n[22] \"  sum <- sum(xx^2)\"                                                                                                                                              \n[23] \"\"                                                                                                                                                                ",
        "\n[24] \"  y <- sum\"                                                                                                                                                      \n[25] \"  return(y)\"                                                                                                                                                     \n[26] \"}\"                                                                                                                                                               \n\n$`./R/spherefmod.R`\n",
        " [1] \"#' SPHERE FUNCTION, MODIFIED\"                                                                                                                                    \n [2] \"#'\"                                                                                                                                                              \n [3] \"#' Dimensions: d\"                                                                                                                                                \n [4] \"#' \\tThe Sphere function has d local minima except for the global one. It is continuous, convex and unimodal. The plot shows its two-dimensional form.\"          ",
        "\n [5] \"#' \\tThe function is usually evaluated on the hypercube xi <U+2208> [-5.12, 5.12], for all i = 1, ?, d.\"                                                         \n [6] \"#' \\tModifications and Alternate Forms:\"                                                                                                                         \n [7] \"#' \\tPicheny et al. (2012) use the following, slightly different, version of the Sphere function, with d = 6, on [0, 1]6:\"                                       \n [8] \"#' \\tThis function has a mean of zero and a variance of one. The authors also add a small Gaussian error term to the output.\"                                    ",
        "\n [9] \"#' @references \\\\url{https://www.sfu.ca/~ssurjano/spheref.html}\"                                                                                                 \n[10] \"#' \\tDixon, L. C. W., & Szego, G. P. (1978). The global optimization problem: an introduction. Towards global optimization, 2, 1-15.\"                            \n[11] \"#' \\tMolga, M., & Smutnicki, C. Test functions for optimization needs (2005). Retrieved June 2013, from http://www.zsd.ict.pwr.wroc.pl/files/docs/functions.pdf.\"\n[12] \"#' \\tPicheny, V., Wagner, T., & Ginsbourger, D. (2012). A benchmark of kriging-based infill criteria for noisy optimization.\"                                    ",
        "\n[13] \"#' @author Sonja Surjanovic, Simon Fraser University\"                                                                                                            \n[14] \"#'          \\tDerek Bingham, Simon Fraser University\"                                                                                                            \n[15] \"#' @param xx c(x1, x2, x3, x4, x5, x6)\"                                                                                                                          \n[16] \"#' @export spherefmod\"                                                                                                                                           ",
        "\n[17] \"#'\"                                                                                                                                                              \n[18] \"\"                                                                                                                                                                \n[19] \"spherefmod <- function(xx)\"                                                                                                                                      \n[20] \"{\"                                                                                                                                                               ",
        "\n[21] \"\"                                                                                                                                                                \n[22] \"  ii <- c(1:6)\"                                                                                                                                                  \n[23] \"  sum <- sum((xx^2)*(2^ii))\"                                                                                                                                     \n[24] \"\"                                                                                                                                                                ",
        "\n[25] \"  y <- (sum - 1745) / 899\"                                                                                                                                       \n[26] \"  return(y)\"                                                                                                                                                     \n[27] \"}\"                                                                                                                                                               \n\n$`./R/stybtang.R`\n",
        " [1] \"#' STYBLINSKI-TANG FUNCTION\"                                                                      \n [2] \"#'\"                                                                                               \n [3] \"#' Dimensions: d\"                                                                                 \n [4] \"#' \\tThe Styblinski-Tang function is shown here in its two-dimensional form.\"                     \n [5] \"#' \\tThe function is usually evaluated on the hypercube xi <U+2208> [-5, 5], for all i = 1, ?, d.\"",
        "\n [6] \"#' \\tReference:\"                                                                                  \n [7] \"#' @author Sonja Surjanovic, Simon Fraser University\"                                             \n [8] \"#'          \\tDerek Bingham, Simon Fraser University\"                                             \n [9] \"#' @param xx c(x1, x2, ..., xd)\"                                                                  \n[10] \"#' @export stybtang\"                                                                              ",
        "\n[11] \"#'\"                                                                                               \n[12] \"\"                                                                                                 \n[13] \"stybtang <- function(xx)\"                                                                         \n[14] \"{\"                                                                                                \n[15] \"\"                                                                                                 ",
        "\n[16] \"  sum <- sum(xx^4 - 16*xx^2 + 5*xx)\"                                                              \n[17] \"\"                                                                                                 \n[18] \"  y <- sum/2\"                                                                                     \n[19] \"  return(y)\"                                                                                      \n[20] \"}\"                                                                                                ",
        "\n\n$`./R/sumpow.R`\n [1] \"#' SUM OF DIFFERENT POWERS FUNCTION\"                                                                                                                             \n [2] \"#'\"                                                                                                                                                              \n [3] \"#' Dimensions: d\"                                                                                                                                                ",
        "\n [4] \"#' \\tThe Sum of Different Powers function is unimodal. It is shown here in its two-dimensional form.\"                                                            \n [5] \"#' \\tThe function is usually evaluated on the hypercube xi <U+2208> [-1, 1], for all i = 1, ?, d.\"                                                               \n [6] \"#' \\tReference:\"                                                                                                                                                 \n [7] \"#' \\tMolga, M., & Smutnicki, C. Test functions for optimization needs (2005). Retrieved June 2013, from http://www.zsd.ict.pwr.wroc.pl/files/docs/functions.pdf.\"",
        "\n [8] \"#' @author Sonja Surjanovic, Simon Fraser University\"                                                                                                            \n [9] \"#'          \\tDerek Bingham, Simon Fraser University\"                                                                                                            \n[10] \"#' @param xx c(x1, x2, ..., xd)\"                                                                                                                                 \n[11] \"#' @export sumpow\"                                                                                                                                               ",
        "\n[12] \"#'\"                                                                                                                                                              \n[13] \"\"                                                                                                                                                                \n[14] \"sumpow <- function(xx)\"                                                                                                                                          \n[15] \"{\"                                                                                                                                                               ",
        "\n[16] \"\"                                                                                                                                                                \n[17] \"  ii <- c(1:length(xx))\"                                                                                                                                         \n[18] \"  sum <- sum((abs(xx))^(ii+1))\"                                                                                                                                  \n[19] \"\"                                                                                                                                                                ",
        "\n[20] \"  y <- sum\"                                                                                                                                                      \n[21] \"  return(y)\"                                                                                                                                                     \n[22] \"}\"                                                                                                                                                               \n\n$`./R/sumsqu.R`\n",
        " [1] \"#' SUM SQUARES FUNCTION\"                                                                                                                                                                                                        \n [2] \"#'\"                                                                                                                                                                                                                             \n [3] \"#' Dimensions: d\"                                                                                                                                                                                                               ",
        "\n [4] \"#' \\tThe Sum Squares function, also referred to as the Axis Parallel Hyper-Ellipsoid function, has no local minimum except the global one. It is continuous, convex and unimodal. It is shown here in its two-dimensional form.\"\n [5] \"#' \\tThe function is usually evaluated on the hypercube xi <U+2208> [-10, 10], for all i = 1, ?, d, although this may be restricted to the hypercube xi <U+2208> [-5.12, 5.12], for all i = 1, ?, d.\"                           \n [6] \"#' @references \\\\url{https://www.sfu.ca/~ssurjano/sumsqu.html}\"                                                                                                                                                                 ",
        "\n [7] \"#' \\thttp://www-optima.amp.i.kyoto-u.ac.jp/member/student/hedar/Hedar_files/TestGO.htm.\"                                                                                                                                        \n [8] \"#' \\tMolga, M., & Smutnicki, C. Test functions for optimization needs (2005). Retrieved June 2013, from http://www.zsd.ict.pwr.wroc.pl/files/docs/functions.pdf.\"                                                               \n [9] \"#' @author Sonja Surjanovic, Simon Fraser University\"                                                                                                                                                                           ",
        "\n[10] \"#'          \\tDerek Bingham, Simon Fraser University\"                                                                                                                                                                           \n[11] \"#' @param xx c(x1, x2, ..., xd)\"                                                                                                                                                                                                \n[12] \"#' @export sumsqu\"                                                                                                                                                                                                              ",
        "\n[13] \"#'\"                                                                                                                                                                                                                             \n[14] \"\"                                                                                                                                                                                                                               \n[15] \"sumsqu <- function(xx)\"                                                                                                                                                                                                         ",
        "\n[16] \"{\"                                                                                                                                                                                                                              \n[17] \"\"                                                                                                                                                                                                                               \n[18] \"  ii <- c(1:length(xx))\"                                                                                                                                                                                                        ",
        "\n[19] \"  sum <- sum(ii*xx^2)\"                                                                                                                                                                                                          \n[20] \"\"                                                                                                                                                                                                                               \n[21] \"  y <- sum\"                                                                                                                                                                                                                     ",
        "\n[22] \"  return(y)\"                                                                                                                                                                                                                    \n[23] \"}\"                                                                                                                                                                                                                              \n\n$`./R/trid.R`\n [1] \"#' TRID FUNCTION\"                                                                                                                                                                                      ",
        "\n [2] \"#'\"                                                                                                                                                                                                    \n [3] \"#' Dimensions: d\"                                                                                                                                                                                      \n [4] \"#' \\tThe Trid function has no local minimum except the global one. It is shown here in its two-dimensional form.\"                                                                                      ",
        "\n [5] \"#' \\tThe function is usually evaluated on the hypercube xi <U+2208> [-d2, d2], for all i = 1, ?, d.\"                                                                                                   \n [6] \"#' @references \\\\url{https://www.sfu.ca/~ssurjano/trid.html}\"                                                                                                                                          \n [7] \"#' \\tAdorio, E. P., & Diliman, U. P. MVF - Multivariate Test Functions Library in C for Unconstrained Global Optimization (2005). Retrieved August 2017, from http://www.geocities.ws/eadorio/mvf.pdf.\"",
        "\n [8] \"#' \\thttp://www-optima.amp.i.kyoto-u.ac.jp/member/student/hedar/Hedar_files/TestGO.htm.\"                                                                                                               \n [9] \"#' @author Sonja Surjanovic, Simon Fraser University\"                                                                                                                                                  \n[10] \"#'          \\tDerek Bingham, Simon Fraser University\"                                                                                                                                                  ",
        "\n[11] \"#' @param xx c(x1, x2, ..., xd)\"                                                                                                                                                                       \n[12] \"#' @export trid\"                                                                                                                                                                                       \n[13] \"#'\"                                                                                                                                                                                                    ",
        "\n[14] \"\"                                                                                                                                                                                                      \n[15] \"trid <- function(xx)\"                                                                                                                                                                                  \n[16] \"{\"                                                                                                                                                                                                     ",
        "\n[17] \"\"                                                                                                                                                                                                      \n[18] \"  d <- length(xx)\"                                                                                                                                                                                     \n[19] \"  xi <- xx[2:d]\"                                                                                                                                                                                       ",
        "\n[20] \"  xold <- xx[1:(d-1)]\"                                                                                                                                                                                 \n[21] \"\"                                                                                                                                                                                                      \n[22] \"  sum1 = (xx[1]-1)^2 + sum((xi-1)^2)\"                                                                                                                                                                  ",
        "\n[23] \"  sum2 <- sum(xi*xold)\"                                                                                                                                                                                \n[24] \"\"                                                                                                                                                                                                      \n[25] \"  y <- sum1 - sum2\"                                                                                                                                                                                    ",
        "\n[26] \"  return(y)\"                                                                                                                                                                                           \n[27] \"}\"                                                                                                                                                                                                     \n\n$`./R/utils.R`\n  [1] \"\"                                                                                    ",
        "\n  [2] \"\"                                                                                    \n  [3] \"#' @export\"                                                                          \n  [4] \"\"                                                                                    \n  [5] \"method_compare <- function(fun,low, up, ..., budget = 50, p = NULL,\"                 \n  [6] \"                           maximize = FALSE, reps = 20L, nsteps = 5, plt = FALSE,\"   \n  [7] \"                           increament_rate= 0, file = NULL){\"                        ",
        "\n  [8] \"  fun_name <- gsub(\\\"\\\\\\\\W\\\", '', deparse1(substitute(fun)))\"                        \n  [9] \"\"                                                                                    \n [10] \"  library(ggplot2)\"                                                                  \n [11] \"  optimal <- NULL\"                                                                   \n [12] \"  if(missing(low)){\"                                                                 \n [13] \"    dom <- domain(fun)\"                                                              ",
        "\n [14] \"    fun <- getFromNamespace(fun, 'egoOptim')\"                                        \n [15] \"    if(is.function(dom)){\"                                                           \n [16] \"      if(is.null(p)) stop('the dimension `p` must be provided for ', f)\"             \n [17] \"      else dom <- dom(p)\"                                                            \n [18] \"    }\"                                                                               \n [19] \"    optimal <- dom$opt$f\"                                                            ",
        "\n [20] \"    if(is.matrix(optimal)) optimal <- optimal[1,]\"                                   \n [21] \"    low <- if(!is.null(dom$low))dom$low else rep(0, p)\"                              \n [22] \"    up <- if(!is.null(dom$up))dom$up else rep(1, p)\"                                 \n [23] \"  }\"                                                                                 \n [24] \"  if(maximize & is.null(optimal)) optimal <- -1\"                                     \n [25] \"  res <- setNames(vector('list', 3), c('RSO', 'EGO', 'TREGO'))\"                      ",
        "\n [26] \"  for(i in seq_len(reps)){\"                                                          \n [27] \"    X <- lhs::maximinLHS(5*length(low), length(low))\"                                \n [28] \"    X1 <- mapply(scales::rescale, data.frame(X),data.frame(rbind(low, up)))\"         \n [29] \"    y1 <- apply(X1, 1, function(x) (-1)^(maximize)*fun(x, ...))\"                     \n [30] \"    cat(\\\"RSO ITERATION:\\\", i, \\\"\\\\n\\\")\"                                             \n [31] \"    res[['RSO']][[i]] <- optimize_fun(fun, low, up,...,\"                             ",
        "\n [32] \"                                      increament_rate = 0,\"                          \n [33] \"                                      X = X1,y=y1,\"                                  \n [34] \"                                      budget = budget,\"                              \n [35] \"                                      maximize = maximize,\"                          \n [36] \"                                      trueglobal = optimal,\"                         \n [37] \"                                      nsteps = nsteps,\"                              ",
        "\n [38] \"                                      do_maxit = TRUE,basicEGO = FALSE)\"             \n [39] \"    cat(\\\"EGO ITERATION:\\\", i, \\\"\\\\n\\\")\"                                             \n [40] \"    res[['EGO']][[i]] <- optimize_fun(fun, low, up,..., X = X1, y=y1,\"               \n [41] \"                                      budget = budget,\"                              \n [42] \"                                      maximize = maximize,\"                          \n [43] \"                                      nsteps = nsteps,\"                              ",
        "\n [44] \"                                      trueglobal = optimal,\"                         \n [45] \"                                      do_maxit = TRUE,basicEGO = TRUE)\"              \n [46] \"    cat(\\\"TREGO ITERATION:\\\", i, \\\"\\\\n\\\")\"                                           \n [47] \"    res[['TREGO']][[i]] <- optimize_fun(fun, low, up,...,X = X1,y=y1,\"               \n [48] \"                                        trueglobal = optimal,\"                       \n [49] \"                                        maximize = maximize,\"                        ",
        "\n [50] \"                                        budget = budget,\"                            \n [51] \"                                        nsteps = nsteps,\"                            \n [52] \"                                        do_maxit = TRUE,basicEGO = TRUE,\"            \n [53] \"                                        method = 'TREGO')\"                           \n [54] \"    if(increament_rate>0){\"                                                          \n [55] \"      cat(\\\"RSO\\\",increament_rate, \\\"ITERATION: \\\", i, \\\"\\\\n\\\", sep=\\\"\\\")\"           ",
        "\n [56] \"      res[['TREGO']][[i]] <- optimize_fun(fun, low, up,...,X = X1,y=y1,\"             \n [57] \"                                          increament_rate = increament_rate,\"        \n [58] \"                                          trueglobal = optimal,\"                     \n [59] \"                                          maximize = maximize,\"                      \n [60] \"                                          budget = budget,\"                          \n [61] \"                                          nsteps = nsteps,\"                          ",
        "\n [62] \"                                          do_maxit = TRUE,basicEGO = TRUE,\"          \n [63] \"                                          method = 'TREGO')\"                         \n [64] \"    }\"                                                                               \n [65] \"  }\"                                                                                 \n [66] \"  r <- lapply(res, \\\\(x){\"                                                           \n [67] \"    vals <- sapply(x, getElement, 'errors')\"                                         ",
        "\n [68] \"    vals <- if(maximize) 1- vals else log10(vals)\"                                   \n [69] \"    y <- data.frame(t(apply(vals, 1,\"                                                \n [70] \"                            \\\\(y)c(mean = mean(y), sd = sd(y)))))\"                   \n [71] \"    y})\"                                                                             \n [72] \"  d <- transform(array2DF(structure(r, dim = 3)), point = 5*seq(0,nrow(r[[1]])-1))\"  \n [73] \"  assign(fun_name, d)\"                                                               ",
        "\n [74] \"  if(!is.null(file)) write.csv(d, file = file)\"                                      \n [75] \"  list(res=res, plot = plotComparison(d))\"                                           \n [76] \"}\"                                                                                   \n [77] \"\"                                                                                    \n [78] \"#branin_results <- method_compare('branin', budget = 35)\"                            \n [79] \"# save(branin_results, file = \\\"data/branin_results.rda\\\")\"                          ",
        "\n [80] \"#\"                                                                                   \n [81] \"# hart3_results <- my_fn('hart3', budget = 80)\"                                      \n [82] \"# save(hart3_results, file = \\\"data/hart3_results.rda\\\")\"                            \n [83] \"#\"                                                                                   \n [84] \"# hart6_results <- my_fn('hart6', budget = 80)\"                                      \n [85] \"# save(hart6_results, file = \\\"data/hart6_results.rda\\\")\"                            ",
        "\n [86] \"#\"                                                                                   \n [87] \"# camel3_results <- my_fn('camel3', budget = 80)\"                                    \n [88] \"# save(camel3_results, file = \\\"data/camel3_results.rda\\\")\"                          \n [89] \"#\"                                                                                   \n [90] \"# camel6_results <- my_fn('camel6', budget = 80)\"                                    \n [91] \"# save(camel6_results, file = \\\"data/camel6_results.rda\\\")\"                          ",
