        "\n[110] \"# accoustic_results <- my_fn(svm_accuracy, c(2^-16, 2^-6), c(2^6, 2^16),\"            \n[111] \"#             param_names = c('gamma', 'cost'),\"                                     \n[112] \"#             object = svm(Class~., accoustic, kernel = 'radial'),\"                  \n[113] \"#             maximize = TRUE)\"                                                      \n[114] \"\"                                                                                    \n[115] \"# seed_results <- my_fn(svm_accuracy, c(2^-16, 2^-6), c(2^6, 2^16),\"                 ",
        "\n[116] \"#                        param_names = c('gamma', 'cost'),\"                          \n[117] \"#                        object = svm(Type~., seeds, kernel = 'radial'),\"            \n[118] \"#                        maximize = TRUE)\"                                           \n[119] \"#\"                                                                                   \n[120] \"\"                                                                                    \n[121] \"#' @export\"                                                                          ",
        "\n[122] \"\"                                                                                    \n[123] \"plotComparison <- function(res, n = NULL, maximize = FALSE, m=0){\"                   \n[124] \"  if(!is.data.frame(res)){\"                                                          \n[125] \"    r <- lapply(res, \\\\(x){\"                                                         \n[126] \"      vals <- sapply(x, getElement, 'errors')\"                                       \n[127] \"      vals <- if(maximize) 1- vals else log10(vals)\"                                 ",
        "\n[128] \"      y <- data.frame(t(apply(vals, 1,\"                                              \n[129] \"                              \\\\(y)c(mean = mean(y), sd = sd(y)))))\"                 \n[130] \"      y})\"                                                                           \n[131] \"    d <- transform(array2DF(structure(r, dim = 3)), point = 5*seq(0,nrow(r[[1]])-1))\"\n[132] \"  }\"                                                                                 \n[133] \"  else d <- res\"                                                                     ",
        "\n[134] \"  if(is.null(n)) n <- max(d$point)\"                                                  \n[135] \"  d <- subset(d, point>=m & point<=n)\"                                               \n[136] \"  ggplot(d, aes(x = point, mean, color = Var1))+\"                                    \n[137] \"    geom_point() +\"                                                                  \n[138] \"    geom_line(linewidth = 1) +\"                                                      \n[139] \"    geom_errorbar(aes(ymax = (if(maximize)pmin(mean + sd,1) else mean+sd),\"          ",
        "\n[140] \"                      ymin =(if(maximize) pmax(mean - sd, 0) else mean - sd)),\"      \n[141] \"                  linewidth = 1, width = 1, alpha=0.5) +\"                            \n[142] \"    labs(color = 'Method') +\"                                                        \n[143] \"    ylab(if(maximize)'accuracy' else bquote(Log[10]* ' Loss')) +\"                    \n[144] \"    xlab('Total points Used') +\"                                                     \n[145] \"    scale_x_continuous(labels=~.x+10, breaks = ~seq(0, .x[2],5))\"                    ",
        "\n[146] \"}\"                                                                                   \n[147] \"\"                                                                                    \n[148] \"\"                                                                                    \n[149] \"\"                                                                                    \n[150] \"\"                                                                                    \n[151] \"#' @export\"                                                                          ",
        "\n[152] \"#'\"                                                                                  \n[153] \"my_contour <- function(x1, x2, Z, xlab = NULL,\"                                      \n[154] \"                       ylab = NULL, title = NULL,\"                                   \n[155] \"                       background = \\\"white\\\", title.adjust = 0.5,\"                  \n[156] \"                       subtitle = \\\"it1\\\",nlevels=100, col='blue', ...){\"            \n[157] \"  par(bg = background,\"                                                              ",
        "\n[158] \"      adj=0.5,\"                                                                      \n[159] \"      tck = -0.02,\"                                                                  \n[160] \"      #mgp = c(1.5, 0.4, 0), tcl = -0.25,\"                                           \n[161] \"      ## Shrink the tick labels.\"                                                    \n[162] \"      cex.axis = 0.8,\"                                                               \n[163] \"      ## Set the axis label color\"                                                   ",
        "\n[164] \"      col.lab = \\\"black\\\",\"                                                          \n[165] \"      ## Adjust the margin:  bottom, left, top, right\"                               \n[166] \"      mar = c(2, 2.5, 2, 1.2)-c(0,0,is.null(title), 0),\"                             \n[167] \"      bty = \\\"n\\\",\"                                                                  \n[168] \"      mgp = c(1, 0.2, 0))\"                                                           \n[169] \"\"                                                                                    ",
        "\n[170] \"  if(is.null(xlab)) xlab <- deparse1(substitute(x1))\"                                \n[171] \"  if(is.null(ylab)) ylab <- deparse1(substitute(x2))\"                                \n[172] \"  contour(x1, x2, Z, xaxt = \\\"n\\\", yaxt = \\\"n\\\")\"                                    \n[173] \"\"                                                                                    \n[174] \"  rect(par(\\\"usr\\\")[1], par(\\\"usr\\\")[3],\"                                            \n[175] \"       par(\\\"usr\\\")[2], par(\\\"usr\\\")[4],\"                                            ",
        "\n[176] \"       col = \\\"#ebebeb\\\", border = background)\"                                      \n[177] \"\"                                                                                    \n[178] \"  grid(nx = 5, ny = 5,\"                                                              \n[179] \"       lty = 1,      # Grid line type\"                                               \n[180] \"       col = \\\"white\\\", # Grid line color\"                                           \n[181] \"       lwd = 2)      # Grid line width\"                                              ",
        "\n[182] \"  contour(x1, x2, Z, add = TRUE, nlevels = nlevels,...)\"                             \n[183] \"\"                                                                                    \n[184] \"  #box(\\\"plot\\\", bty = \\\"l\\\", lwd = 2)\"                                              \n[185] \"  axis(side = 1, lwd = 0, lwd.ticks = 1, mgp=c(1, 0.2, 0))\"                          \n[186] \"  axis(side = 2, lwd = 0, lwd.ticks = 1, las = 2, mgp=c(1.5, 0.4, 0))\"               \n[187] \"  title(main = title, xlab = xlab, ylab=ylab,\"                                       ",
        "\n[188] \"        adj = title.adjust, sub = subtitle)\"                                         \n[189] \"\"                                                                                    \n[190] \"}\"                                                                                   \n[191] \"\"                                                                                    \n[192] \"#' @export\"                                                                          \n[193] \"\"                                                                                    ",
        "\n[194] \"add_budget <- function(object, ...){\"                                                \n[195] \"  UseMethod('add_budget')\"                                                           \n[196] \"}\"                                                                                   \n[197] \"\"                                                                                    \n[198] \"add_budget.egoOptim <- function(object, its, new_budget){\"                           \n[199] \"  list2env(mget(names(object$env), object$env), environment())\"                      ",
        "\n[200] \"  if(!missing(its)) maxit <- its\"                                                    \n[201] \"  if(!missing(new_budget)) {\"                                                        \n[202] \"    maxit <- new_budget %/% nsteps\"                                                  \n[203] \"    do_maxit <- TRUE\"                                                                \n[204] \"  }\"                                                                                 \n[205] \"  new_err <- errors\"                                                                 ",
        "\n[206] \"  errors <- 0\"                                                                       \n[207] \"  v <- rev(body(optimize_fun))[1:2]\"                                                 \n[208] \"  eval(v[[2]])\"                                                                      \n[209] \"  results <- eval(v[[1]])\"                                                           \n[210] \"  results$errors <- c(error_init, new_err, errors)\"                                  \n[211] \"  results\"                                                                           ",
        "\n[212] \"}\"                                                                                   \n[213] \"\"                                                                                    \n[214] \"add_budget.list <- function(object, its, new_budget, inplace = FALSE){\"              \n[215] \"  nms <-  deparse1(substitute(object))\"                                              \n[216] \"  for(i in names(object$res)){\"                                                      \n[217] \"    for(j in seq_along(object$res[[i]])){\"                                           ",
        "\n[218] \"      cat(\\\"updating\\\", i, \\\"it:\\\", j, \\\"\\\\n\\\")\"                                     \n[219] \"      object$res[[i]][[j]] <- add_budget(object$res[[i]][[j]],\"                      \n[220] \"                                         its = its, new_budget = new_budget)\"        \n[221] \"    }\"                                                                               \n[222] \"  }\"                                                                                 \n[223] \"  object$plot <- plotComparison(object$res)\"                                         ",
        "\n[224] \"  if(inplace)\"                                                                       \n[225] \"    setNames(list(object), nms)|>\"                                                   \n[226] \"    list2env(parent.frame())|>\"                                                      \n[227] \"    invisible()\"                                                                     \n[228] \"  else object\"                                                                       \n[229] \"}\"                                                                                   ",
        "\n[230] \"\"                                                                                    \n[231] \"#' @export\"                                                                          \n[232] \"add_replicates <- function(call, reps){\"                                             \n[233] \"\"                                                                                    \n[234] \"  a <- call$call$reps\"                                                               \n[235] \"  call$call$reps <- reps\"                                                            ",
        "\n[236] \"  res <- Map(c, call$res, eval(call$call)$res)\"                                      \n[237] \"  call$call$reps <- reps + a\"                                                        \n[238] \"  v <- mget(c('maximize','nsteps'), call$res[[1]][[1]]$env)\"                         \n[239] \"  list(res = res,\"                                                                   \n[240] \"       plot = plotComparison(res, maximize = v$maximize,\"                            \n[241] \"                             nsteps = v$nsteps,\"                                     ",
        "\n[242] \"                             errorbars = !v$maximize),\"                              \n[243] \"       call=call$call)\"                                                              \n[244] \"}\"                                                                                   \n[245] \"\"                                                                                    \n\n$`./R/zakharov.R`\n [1] \"#' ZAKHAROV FUNCTION\"                                                                                               ",
        "\n [2] \"#'\"                                                                                                                 \n [3] \"#' Dimensions: d\"                                                                                                   \n [4] \"#' \\tThe Zakharov function has no local minima except the global one. It is shown here in its two-dimensional form.\"\n [5] \"#' \\tThe function is usually evaluated on the hypercube xi <U+2208> [-5, 10], for all i = 1, ?, d.\"                 \n [6] \"#' \\tReference:\"                                                                                                    ",
        "\n [7] \"#' \\thttp://www-optima.amp.i.kyoto-u.ac.jp/member/student/hedar/Hedar_files/TestGO.htm.\"                            \n [8] \"#' @author Sonja Surjanovic, Simon Fraser University\"                                                               \n [9] \"#'          \\tDerek Bingham, Simon Fraser University\"                                                               \n[10] \"#' @param xx c(x1, x2, ..., xd)\"                                                                                    \n[11] \"#' @export zakharov\"                                                                                                ",
        "\n[12] \"#'\"                                                                                                                 \n[13] \"\"                                                                                                                   \n[14] \"zakharov <- function(xx)\"                                                                                           \n[15] \"{\"                                                                                                                  \n[16] \"\"                                                                                                                   ",
        "\n[17] \"  ii <- c(1:length(xx))\"                                                                                            \n[18] \"  sum1 <- sum(xx^2)\"                                                                                                \n[19] \"  sum2 <- sum(0.5*ii*xx)\"                                                                                           \n[20] \"\"                                                                                                                   \n[21] \"  y <- sum1 + sum2^2 + sum2^4\"                                                                                      ",
        "\n[22] \"  return(y)\"                                                                                                        \n[23] \"}\"                                                                                                                  \n\n",
