        "\n [5] \"\\\\title{ROSENBROCK FUNCTION}\"                                                                                                                                                                                  \n [6] \"\\\\usage{\"                                                                                                                                                                                                      \n [7] \"rosen(xx)\"                                                                                                                                                                                                     ",
        "\n [8] \"}\"                                                                                                                                                                                                             \n [9] \"\\\\arguments{\"                                                                                                                                                                                                  \n[10] \"\\\\item{xx}{c(x1, x2, ..., xd)}\"                                                                                                                                                                                ",
