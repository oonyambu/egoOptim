list.files()
list.files(all.files = TRUE)
list.files(all.files = TRUE, recursive = TRUE)
list.files(all.files = TRUE, recursive = TRUE, full.names = TRUE)
sapply(list.files(all.files = TRUE, recursive = TRUE, full.names = TRUE), readLines)
warnings()
sapply(list.files(all.files = TRUE, recursive = TRUE, full.names = TRUE), \(x)grepl("plotComparison", toString(readLines(x))))
sapply(list.files(all.files = TRUE, recursive = TRUE, full.names = TRUE), \(x)grepl("plotComparison", toString(readLines(x))))|>which()
remotes::install_github('oonyambu/egoOptim')
sapply(list.files(all.files = TRUE, recursive = TRUE, full.names = TRUE), \(x)grepl("plotComparison\\(res", toString(readLines(x))))|>which()
sapply(list.files(all.files = TRUE, recursive = TRUE, full.names = TRUE), \(x)grepl("plotComparison\\(res, *maximize *= *v\\$maximize", toString(readLines(x))))|>which()
remove.packages("egoOptim")
